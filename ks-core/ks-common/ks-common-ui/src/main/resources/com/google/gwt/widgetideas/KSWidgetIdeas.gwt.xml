<module>
	<!--
	This is the KS version of the gwt incubator widgetideas module config.
	The intent is to only pull in widgetideas classes directly/indirectly used by KS.
	Doing so allows us to cut down on unnecessary gwt compile warnings for using deprecated classes
	by classes found in widgetideas package. 
	 -->
	<source>
		<include name="**/*ProgressBar.java"/>
		<include name="**/*ResizableWidget*.java"/>		
	</source>
</module>