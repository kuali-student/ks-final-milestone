       </property>
      </bean>
      <bean parent="whenConstraint">
       <property name="values">
        <list>
         <value>kuali.reqComponent.field.type.operator</value>
        </list>
       </property>
       <property name="constraint">
        <bean parent="reqCompFieldInfo.value.operator"/>
       </property>
      </bean>
      <bean parent="whenConstraint">
       <property name="values">
