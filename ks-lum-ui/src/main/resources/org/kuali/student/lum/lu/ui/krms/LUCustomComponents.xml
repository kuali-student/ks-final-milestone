    <bean id="KRMS-Program-InputField" parent="KRMS-Program-InputField-Parent"/>

    <bean id="KRMS-ProgramWithNofAndOrg-Group" parent="KRMS-CustomComponent-BoxGroup">
        <property name="items">
            <list>
                <ref bean="KRMS-Program-Section"/>
