    <bean id="KRMS-GradeValues-Field" parent="KRMS-GradeValues-Field-Parent"/>

    <bean id="KRMS-Grade-Section-Parent" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <ref bean="KRMS-GradeScale-Field"/>
                <ref bean="KRMS-GradeValues-Field"/>
