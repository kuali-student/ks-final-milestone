<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xmlns:cxf="http://cxf.apache.org/core"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd
http://cxf.apache.org/core http://cxf.apache.org/schemas/core.xsd">

	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />
	<import resource="classpath:ks-core-context.xml" />

	<jaxws:endpoint id="orgService"
		implementorClass="org.kuali.student.core.organization.service.impl.OrganizationServiceImpl"
		implementor="#orgServiceImpl" address="/OrgService" />

	<jaxws:endpoint id="emService"
		implementorClass="org.kuali.student.core.enumerationmanagement.service.impl.EnumerationManagementServiceImpl"
		implementor="#emServiceImpl" address="/EmService" />

	<jaxws:endpoint id="messageService"
		implementorClass="org.kuali.student.core.messages.service.impl.MessageServiceImpl"
		implementor="#messageServiceImpl" address="/MessageService" />

    <jaxws:endpoint id="commentService"
        implementorClass="org.kuali.student.core.comment.service.impl.CommentServiceImpl"
        implementor="#commentServiceImpl" address="/CommentService" />

	<!--
	<cxf:bus>
		<cxf:features>
			<cxf:logging />
		</cxf:features>
	</cxf:bus>
	-->

</beans>