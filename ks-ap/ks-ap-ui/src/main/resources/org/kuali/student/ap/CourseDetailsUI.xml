                    <property name="onDocumentReadyScript" value="ksapRetrieveComponent('CourseDetails-InquiryView-CourseSection-Holder','coursedetails-page','startCourseSectionDetails','course/details',{courseId:'@{dataObject.courseId}'}, null, {message:'&lt;div class=&quot;ksap-loader&quot;&gt;Loading...&lt;/div&gt;Loading...', fadeIn:0, fadeOut:0, overlayCSS:{backgroundColor:'#fff',opacity:0,cursor:'wait'}, css:{border:'0'}});"/>
