    <bean id="KS-CourseOfferingManagement-SelectedAOCSRConfirmationPage" parent="KS-Uif-Page"
          p:id="selectedAOCSRConfirmationPage">

        <!-- This was originally put in as a work-around for a KRAD unified header bug where view headers weren't being updated
             when pages where refreshed. I investigated and partially implemented handing view title text overrides in the controller
             code but concluded that this is a much cleaner solution. -->
        <property name="onDocumentReadyScript" value= "updateViewHeaderText('@{currentCourseOfferingWrapper.courseOfferingCode}');"/>
        <property name="headerText" value="@{csrLabel} Activity Offerings" />

        <property name="breadcrumbOptions.homewardPathBreadcrumbs">
            <util:list id="KS-Enrollment-HomewardPathBreadcrumbs">
                <ref bean="KS-HomewardPathBreadcrumbs-Home"/>
                <ref bean="KS-HomewardPathBreadcrumbs-Enrollment"/>
                <ref bean="KS-HomewardPathBreadcrumbs-ManageCo-Landing"/>
                <ref bean="KS-HomewardPathBreadcrumbs-Search-Subject"/>
                <ref bean="KS-HomewardPathBreadcrumbs-ManageOneCo-Page"/>
            </util:list>
        </property>

        <property name="breadcrumbOptions.breadcrumbOverrides">
            <list>
                <bean parent="Uif-BreadcrumbItem">
                    <property name="label" value="@{csrLabel} Activity Offerings" />
                </bean>
            </list>
        </property>

        <property name="items">
            <list>
                <ref bean="KS-CourseOfferingManagement-CSRConfirmationResultSection"/>
            </list>
        </property>

        <property name="footer">
            <bean parent="KS-Uif-FooterBase">
                <property name="items">
                    <list>
                        <bean parent="Uif-HorizontalBoxGroup">
                            <property name="items">
                                <list>
                                    <bean parent="KS-Uif-PrimaryActionButton" p:id="AoCancelConfirmationCancelButton" p:actionLabel="Cancel Activity Offering(s)" p:methodToCall="cancelSelectedAoList" p:render="@{actionCSR == T(org.kuali.student.enrollment.class2.courseoffering.util.ActivityOfferingConstants).ACTIVITYOFFERING_ACTION_CANCEL}"/>
                                    <bean parent="KS-Uif-PrimaryActionButton" p:id="AoSuspendConfirmationCancelButton" p:actionLabel="Suspend Activity Offering(s)" p:methodToCall="suspendSelectedAoList" p:render="@{actionCSR == T(org.kuali.student.enrollment.class2.courseoffering.util.ActivityOfferingConstants).ACTIVITYOFFERING_ACTION_SUSPEND}"/>
                                    <bean parent="KS-Uif-PrimaryActionButton" p:id="AoReinstateConfirmationCancelButton" p:actionLabel="Reinstate Activity Offering(s)" p:methodToCall="reinstateSelectedAoList" p:render="@{actionCSR == T(org.kuali.student.enrollment.class2.courseoffering.util.ActivityOfferingConstants).ACTIVITYOFFERING_ACTION_REINSTATE}"/>
                                    <bean parent="KS-Uif-ActionLink" p:actionLabel="Cancel" p:methodToCall="reloadManageCO" />
                                </list>
                            </property>
                        </bean>
                    </list>
                </property>
            </bean>
        </property>
    </bean>

