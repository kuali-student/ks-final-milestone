<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="KS-EnrollmentHome-View" parent="KS-Uif-FormView">
        <property name="id" value="enrollmentHomeView"/>
        <property name="headerText" value="Enrollment"/>
        <property name="formClass" value="org.kuali.student.enrollment.main.form.LaunchForm"/>
        <property name="footer">
            <bean parent="Uif-FooterBase"/>
        </property>
        <property name="cssClasses">
            <list merge="true">
                <value>ks-enrollment-home</value>
            </list>
        </property>
        <property name="breadcrumbOptions.homewardPathBreadcrumbs">
            <list>
                <bean parent="Uif-BreadcrumbItem">
                    <property name="label" value="Home"/>
                    <property name="url.href" value="@{#ConfigProperties['application.url']}/portal.do"/>
                </bean>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-Page" p:disclosure.render="false">
                    <property name="items">
                        <list>
                            <ref bean="KS-EnrollmentHome-Setup"/>
                            <ref bean="KS-EnrollmentHome-PeopleAndPermissions"/>
                            <ref bean="KS-EnrollmentHome-CourseOfferings"/>
                            <ref bean="KS-EnrollmentHome-Registration"/>
                            <ref bean="KS-EnrollmentHome-Holds"/>
                            <ref bean="KS-EnrollmentHome-Process"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KS-EnrollmentHome-Setup" parent="Uif-VerticalBoxSection">
        <property name="headerText" value="Setup"/>
        <property name="cssClasses">
            <list merge="true">
                <value>ks-enrollment-home-group</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-Link" p:linkText="Search for Calendar or Term"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/calendarSearch?viewId=calendarSearchView&amp;methodToCall=start"/>
                <bean parent="Uif-Link" p:linkText="Create New Calendar (Academic Year)"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/academicCalendar?viewId=academicCalendarFlowView&amp;pageId=academicCalendarCopyPage&amp;methodToCall=startNew"/>
                <bean parent="Uif-Link" p:linkText="Create Holiday Calendar"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/holidayCalendar?viewId=holidayCalendarFlowView&amp;pageId=holidayCalendarCopyPage&amp;methodToCall=startNew"/>
            </list>
        </property>
    </bean>

    <bean id="KS-EnrollmentHome-PeopleAndPermissions" parent="Uif-VerticalBoxSection">
        <property name="headerText" value="People And Permissions"/>
        <property name="cssClasses">
            <list merge="true">
                <value>ks-enrollment-home-group</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-Link" p:linkText="Manage Populations"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/lookup?viewId=KS-Population-LookupView&amp;methodToCall=start&amp;dataObjectClassName=org.kuali.student.enrollment.class2.population.dto.PopulationWrapper&amp;returnLocation=@{#ConfigProperties['application.url']}/portal.do&amp;hideReturnLink=true&amp;showMaintenanceLinks=true&amp;viewName=Manage Populations"/>
            </list>
        </property>
    </bean>

    <bean id="KS-EnrollmentHome-CourseOfferings" parent="Uif-VerticalBoxSection">
        <property name="headerText" value="Course Offerings"/>
        <property name="cssClasses">
            <list merge="true">
                <value>ks-enrollment-home-group</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-Link" p:linkText="Perform Rollover"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/courseOfferingRollover?viewId=courseOfferingRolloverManagementView&amp;pageId=selectTermsForRollover&amp;methodToCall=start"/>
                <bean parent="Uif-Link" p:linkText="View Rollover Details"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/courseOfferingRollover?viewId=courseOfferingRolloverManagementView&amp;pageId=selectTermForRolloverDetails&amp;methodToCall=start"/>
                <bean parent="Uif-Link" p:linkText="Create Course Offerings"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/courseOfferingCreate?methodToCall=start&amp;dataObjectClassName=org.kuali.student.enrollment.class2.courseoffering.dto.CourseOfferingCreateWrapper&amp;hideReturnLink=true"/>
                <bean parent="Uif-Link" p:linkText="Manage Course Offerings"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/courseOfferingManagement?viewId=courseOfferingManagementView&amp;pageId=searchInputPage&amp;methodToCall=start&amp;withinPortal=false"/>
                <bean parent="Uif-Link" p:linkText="Manage SOC"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/manageSOC?viewId=ManageSOCView&amp;methodToCall=start"/>
                <bean parent="Uif-Link" p:linkText="Schedule of Classes"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/scheduleOfClassesSearch?viewId=scheduleOfClassesSearchView&amp;pageId=scheduleOfClassesSearchInputPage&amp;methodToCall=start"/>
            </list>
        </property>
    </bean>

    <bean id="KS-EnrollmentHome-Registration" parent="Uif-VerticalBoxSection">
        <property name="headerText" value="Registration"/>
        <property name="cssClasses">
            <list merge="true">
                <value>ks-enrollment-home-group</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="Manage General Environment" p:style="color:#777"/>
                <bean parent="Uif-Link" p:linkText="Manage Registration Windows and Appointments"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/registrationWindows?viewId=registrationWindowsManagementView&amp;pageId=selectTermForRegWindows&amp;methodToCall=start"/>
                <bean parent="Uif-Message" p:messageText="Assign Registration Appointments" p:style="color:#777"/>
                <bean parent="Uif-Message" p:messageText="Manage Individual Student Appointments" p:style="color:#777"/>
            </list>
        </property>
    </bean>

    <bean id="KS-EnrollmentHome-Holds" parent="Uif-VerticalBoxSection">
        <property name="headerText" value="Holds"/>
        <property name="cssClasses">
            <list merge="true">
                <value>ks-enrollment-home-group</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-Link" p:linkText="Create Hold"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/holdIssueInfoController?viewId=holdCreateView&amp;methodToCall=start"/>
                <bean parent="Uif-Link" p:linkText="Manage Hold"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/holdIssueInfoController?viewId=holdSearchView&amp;methodToCall=start"/>
                <bean parent="Uif-Link" p:linkText="Organization Lookup"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/lookup?methodToCall=start&amp;dataObjectClassName=org.kuali.student.r2.core.organization.dto.OrgInfo&amp;returnLocation=@{#ConfigProperties['application.url']}/portal.do&amp;hideReturnLink=true"/>
            </list>
        </property>
    </bean>
    <bean id="KS-EnrollmentHome-Process" parent="Uif-VerticalBoxSection">
        <property name="headerText" value="Processes"/>
        <property name="cssClasses">
            <list merge="true">
                <value>ks-enrollment-home-group</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-Link" p:linkText="Create Process"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/processInfoController?viewId=processCreateView&amp;methodToCall=start"/>
                <bean parent="Uif-Link" p:linkText="Create Check"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/createCheck?viewId=checkCreateView&amp;methodToCall=start"/>
                <bean parent="Uif-Link" p:linkText="Manage Process"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/processInfoController?viewId=processInfoSearchView&amp;methodToCall=start"/>
                <bean parent="Uif-Link" p:linkText="Manage Check"
                      p:href="@{#ConfigProperties['application.url']}/kr-krad/createCheck?viewId=checkInfoSearchView&amp;methodToCall=start"/>
                <bean parent="Uif-Link" p:linkText="Manage Instruction"
                      p:href=""/>
            </list>
        </property>
    </bean>

</beans>