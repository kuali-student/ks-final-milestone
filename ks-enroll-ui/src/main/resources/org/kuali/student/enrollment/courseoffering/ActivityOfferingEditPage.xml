    <bean id="ActivityOfferingEdit-SeatPool" parent="Uif-VerticalBoxSection" p:additionalCssClasses="ks-seatpools,ks-section">
        <property name="validationMessages.displayMessages" value="false"/>
        <property name="headerText" value="Seat Pools"/>
        <property name="componentSecurity.editAuthz" value="true"/>
        <property name="items">
            <list>
                <bean parent="Uif-MessageField" p:label="Seat Pools" p:messageText="0" p:id="seatpoolCount">
                </bean>
                <bean parent="Uif-MessageField" p:label="Seats Remaining" p:messageText="N/A" p:id="seatsRemaining">
                </bean>
                <bean id="ActivityOffering-SeatpoolsSection" parent="KS-TableCollectionSubSection-BottomAddLine"
                      p:id="ao-seatpoolgroupZ"
                      p:layoutManager.id="ao-seatpoolgroup-table" p:additionalCssClasses="ks-subSection" p:render="@{!(!#dp.hasSeatpools and #parent.readOnly)}">
                    <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.SeatPoolWrapper" />
                    <property name="propertyName" value="seatpools" />
                    <property name="addBlankLineAction.actionLabel" value="Add Seat Pool"/>
                    <property name="addBlankLineAction.onDocumentReadyScript" value="showSPAddlinePriority(); reDisplaySeatPoolSection();"/>
                    <property name="addBlankLineAction.id" value="ao-seatpoolgroupZ_add"/>
                    <property name="addBlankLineAction.dirtyOnAction" value="true" />
                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField" p:id="seatPoolPriority" p:propertyName="seatPool.processingPriority" p:label="Priority"
                                  p:maxLength="2" p:required="true">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="2" />
                                </property>
                                <property name="validCharactersConstraint">
                                    <bean parent="IntegerPatternConstraint" />
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:propertyName="seatPool.seatLimit" p:label="Seats" p:maxLength="3" p:id="seatLimit" >
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="3" p:onBlurScript="validateSeatsForSP(jQuery(this))" />
                                </property>
                                <property name="dependencyConstraints">
                                    <list>
                                        <bean parent="PrerequisiteConstraint" p:propertyName="seatPoolPopulation.name"/>
                                    </list>
                                </property>
                                <property name="validCharactersConstraint">
                                    <bean parent="IntegerPatternConstraint" />
                                </property>
                            </bean>
                            <bean parent="Uif-MessageField" p:messageText="%" p:label="Percent" p:id="seatLimitPercent" />
                            <bean parent="Uif-InputField" p:label="Population Name" p:id="ao-seatpoolgroup-population-name" p:propertyName="seatPoolPopulation.name"
                                  p:onChangeScript="jQuery('#dirtyForm').val(true);" p:cssClasses="col_6">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="40" p:onBlurScript="validatePopulationForSP(this,'@{document.newMaintainableObject.dataObject.populationsJSONString}');"
                                          p:cssClasses="this, validSeatsAndPopulationName"/>
                                </property>
                                <property name="quickfinder">
                                    <bean parent="Uif-QuickFinder" p:lightBoxLookup.lookupReturnByScript="true">
                                        <property name="dataObjectClassName" value="org.kuali.student.enrollment.class2.population.dto.PopulationWrapper"/>
                                        <property name="fieldConversions">
                                            <map>
                                                <entry key="populationInfo.id" value="seatPoolPopulation.id"/>
                                                <entry key="populationInfo.name" value="seatPoolPopulation.name"/>
                                            </map>
                                        </property>
                                    </bean>
                                </property>
                                <property name="dependencyConstraints">
                                    <list>
                                        <bean parent="PrerequisiteConstraint" p:propertyName="seatPool.seatLimit"/>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:id="seatpool_milestone" p:propertyName="seatPool.expirationMilestoneTypeKey" p:onChangeScript="jQuery('#dirtyForm').val(true);" p:label="Expiration Milestone">
                                <property name="control">
                                    <bean parent="Uif-DropdownControl" />
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.SeatPoolExpirationMilestoneTypeKeyValues" p:addBlankOption="false"/>
                                </property>
                            </bean>
                        </list>
                    </property>

                    <property name="lineActions">
                        <list>
                            <bean parent="KS-Uif-Delete-ActionLink" p:methodToCall="deleteLine" p:actionLabel="delete" p:onDocumentReadyScript="calculatePercent()" p:dirtyOnAction="true"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

