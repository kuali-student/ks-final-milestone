    <bean id="ActivityOfferingEdit_SubmitCancel" parent="ActivityOfferingEdit_SubmitCancel-parent"/>
    <bean id="ActivityOfferingEdit_SubmitCancel-parent" parent="Uif-DocumentPageFooter" abstract="true">
        <property name="items">
            <list>
                <bean parent="Uif-HorizontalBoxGroup">
                    <property name="items">
                        <list>
                            <bean parent="KS-Uif-PrimaryActionButton" p:ajaxSubmit="false" p:methodToCall="route" p:actionLabel="Update"
                                p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE]}" p:preSubmitCall="return preSubmitCheck();"
                                p:jumpToIdAfterSubmit="TOP"/>
                            <bean parent="KS-Uif-SecondaryActionButton" p:ajaxSubmit="false" p:methodToCall="route" p:actionLabel="Save Progress"
                                p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE]}" p:preSubmitCall="return preSubmitCheck();"
                                p:jumpToIdAfterSubmit="TOP">
                                <property name="actionParameters">
                                    <map>
                                        <entry key="aoId" value="@{#dp.aoInfo.id}"/>
                                    </map>
                                </property>
                            </bean>
                            <bean parent="Uif-ActionLink"
                                  p:ajaxSubmit="false"
                                  p:methodToCall="cancel"
                                  p:actionLabel="Cancel"
                                  p:additionalCssClasses="cancel_link"
                                  p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CANCEL]}"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-section1" parent="Uif-VerticalBoxSection" p:onDocumentReadyScript="rePositionChangeLink('subterm_name', 'change_link')">
        <property name="items">
            <list>
                <bean parent="Uif-GridGroup" p:layoutManager.numberOfColumns="4" p:additionalCssClasses="ks-details-panel,ks-no-margins,ks-subSection">
                    <property name="items">
                        <list>
                            <bean parent="Uif-DataField" p:colSpan="3" id="status" p:label="Status" p:fieldLabel.renderColon ="false" p:propertyName="stateName"/>
                            <bean parent="Uif-DataField" p:colSpan="3" id="term" p:label="Term" p:fieldLabel.renderColon="false" p:propertyName="termName"/>
                            <bean parent="Uif-DataField" id="subterm_name" p:label="Sub Term" p:fieldLabel.renderColon="false" p:propertyName="subTermName"/>
                            <bean parent="Uif-HorizontalBoxSection" id="aoEdit_subTerm_change"  p:colSpan="2">
                                <property name="header.render" value="false"/>
                                <property name="items">
                                    <list>
                                        <bean parent="Uif-NavigationActionLink" id="change_link" p:actionLabel="Change" p:render="@{#dp.hasSubTerms and !#parent.readOnly}"
                                            p:performDirtyValidation="false" p:actionScript="openPopupContent(e,'KS-ActivityOfferingEdit-SubtermsPopupForm');"
                                            p:componentSecurity.performActionAuthz="true" p:actionEvent="changeSubtermAO"/>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-DataField" p:colSpan="3" id="start_end_date" p:label="Start/End" p:fieldLabel.renderColon="false" p:propertyName="termStartEndDate" />
                            <bean parent="Uif-DataField" id="type_name" p:label="Activity Type" p:fieldLabel.renderColon="false" p:propertyName="typeName" />
                            <bean parent="Uif-DataField"  id="format_offering_name" p:label="Format" p:fieldLabel.renderColon="false" p:propertyName="aoInfo.formatOfferingName" />
                        </list>
                    </property>
                </bean>
                <bean id="KS-ActivityOfferingEdit-SubtermsPopupForm" parent="Uif-HorizontalBoxSection">
                    <property name="hidden" value="true"/>
                    <property name="cssClasses">
                        <list>
                            <value>uif-bubblepopup-content</value>
                        </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean id="subterm_type" parent="Uif-InputField-LabelTop" p:propertyName="subTermId" p:label="Choose a subterm">
                                <property name="control">
                                    <bean parent="Uif-DropdownControl" p:onChangeScript="displayAOsubTerm('subterm_name','subterm_type','change_link','activityCode','@{#dp.subTermDatesJsonString}');"/>
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.ActivityOfferingSubtermKeyValues" p:addBlankOption="false"/>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-section2" parent="Uif-VerticalBoxSection" p:onDocumentReadyScript="calculatePercent()">
        <property name="validationMessages.displayMessages" value="false"/>
        <property name="headerText" value="Activity Offering Information"/>
        <property name="additionalCssClasses">
            <list merge="true">
                <value>ks-subSection</value>
                <value>ks-subSection-marginTop</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-InputField" id="activity_code" p:label="Activity Code " p:fieldLabel.renderColon="false" p:propertyName="aoInfo.activityCode"
                      p:onChangeScript="jQuery('#dirtyForm').val(true);" p:id="activityCode" p:componentSecurity.editAuthz="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="1" p:disabled="@{!#dp.editActivityCodeAllowed}" />
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:propertyName="partOfColoSetOnLoadAlready" p:onChangeScript="jQuery('#dirtyForm').val(true);" p:id="colocatedAO_value" p:readOnlyDisplayReplacement=" ">
                    <property name="control">
                        <bean parent="Uif-HiddenControl"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:propertyName="hiddenMaxEnrollmentShared" p:onChangeScript="jQuery('#dirtyForm').val(true);" p:id="maxEnrollmentShared_value" p:readOnlyDisplayReplacement=" ">
                    <property name="control">
                        <bean parent="Uif-HiddenControl"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:id="infoColoStatus" p:propertyName="aoInfo.isColocated" p:hidden="true" />  <!-- keeps track of whether or not the colocation has been persisted to the db -->
                <bean parent="Uif-InputField-LabelRight"
                      p:label="This activity is co-located"
                      p:fieldLabel.renderColon="false"
                      p:onChangeScript="jQuery('#dirtyForm').val(true);"
                      p:propertyName="colocatedAO"
                      p:id="is_co_located" p:componentSecurity.viewAuthz="true">
                    <property name="control">
                        <bean parent="Uif-CheckboxControl">
                            <property name="disabled" value="@{#dp.aoInfo.stateKey eq 'kuali.lui.activity.offering.state.canceled'}"/>
                            <property name="onChangeScript" value="handleColocation();"/>
                        </bean>
                    </property>
                </bean>
                <bean parent="Uif-InputField" id="max_enrollment" p:label="Total Maximum Enrollment" p:fieldLabel.renderColon="false" p:propertyName="aoInfo.maximumEnrollment"
                      p:id="maximumEnrollment" p:onChangeScript="jQuery('#dirtyForm').val(true); calculatePercent(jQuery(this))" p:componentSecurity.editAuthz="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="3"/>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-CoLocation" parent="Uif-VerticalBoxSection"  p:componentSecurity.viewAuthz="true">
        <property name="additionalCssClasses">
            <list merge="true">
                <value>ks-activity-offering-coLocation-form</value>
                <value>ks-section</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="Select Activities for Colocation"/>
                <ref bean="ActivityOfferingEdit-CoLocatedActivities"/>
                <bean parent="Uif-Message" p:messageText="Set Maximum Enrollment"/>
                <bean id="share_seats" parent="Uif-InputField" p:propertyName="maxEnrollmentShared">
                    <property name="control">
                        <bean parent="Uif-HorizontalRadioControl">
                            <property name="options">
                                <list>
                                    <bean parent="Uif-KeyLabelPair" p:key="true" p:value="Share max enrollment across all activities"/>
                                    <bean parent="Uif-KeyLabelPair" p:key="false" p:value="Manage Enrollments separately"/>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <ref bean="ActivityOfferingEdit-CoLocatedEnrollmentShared"/>
                <ref bean="ActivityOfferingEdit-CoLocatedEnrollmentSeperate"/>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-CoLocatedActivities" parent="Uif-TableCollectionSection">
        <property name="headerText" value=" "/>
        <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.ColocatedActivity" />
        <property name="propertyName" value="colocatedActivities"/>
        <property name="layoutManager.richTable.render" value="false"/>
        <property name="layoutManager.renderSequenceField" value="false" />
        <property name="validationMessages.displayMessages" value="false"/>
        <property name="additionalCssClasses">
            <list merge="true">
                <value>ks-subSection</value>
            </list>
        </property>
        <property name="addLineActions">
            <list>
                <bean parent="Uif-SecondaryActionButton"
                      p:methodToCall="addLine"
                      p:actionLabel="Add"
                      p:actionScript="showLightboxComponent('ActivityOfferingEdit-CoLocatedAddLineDialog');jQuery('#dirtyForm').val(true);"
                      p:dirtyOnAction="true"/>
            </list>
        </property>
        <property name="lineActions">
            <list>
                <bean parent="KS-Uif-Delete-ActionLink" p:methodToCall="deleteLine" p:actionLabel="delete" p:successCallback="addColocatedAOSuccessCallBack();"
                      p:render="@{#dp.requestedScheduleComponents.isEmpty()}" p:dirtyOnAction="true"/>
            </list>
        </property>
        <property name="items">
            <list>
                <!-- CO Input Field -->
                <bean parent="Uif-InputField"
                      p:id="ActivityOfferingEdit-CoLocatedActivities-CoInputField"
                      p:propertyName="courseOfferingCode"
                      p:fieldLabel.render="false"
                      p:uppercaseValue="true"
                      p:required="true"
                      p:readOnly="@{!#isAddLine}">
                    <property name="validCharactersConstraint">
                        <bean parent="AlphaNumericPatternConstraint" />
                    </property>
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="10" p:watermarkText="ENGL101"/>
                    </property>
                    <property name="suggest">
                        <bean parent="Uif-Suggest" p:render="true" p:returnFullQueryObject="true">
                            <property name="suggestQuery">
                                <bean parent="Uif-AttributeQueryConfig" p:queryMethodToCall="retrieveCourseOfferingCodes"
                                      p:queryMethodArgumentFieldList="document.newMaintainableObject.dataObject.termId"/>
                            </property>
                            <property name="templateOptions">
                                <map merge="true">
                                    <entry key="minLength" value="4"/>
                                </map>
                            </property>
                        </bean>
                    </property>
                </bean>

                <!-- AO Input Field -->
                <bean parent="Uif-InputField"
                      p:id="ActivityOfferingEdit-CoLocatedActivities-AoInputField"
                      p:propertyName="activityOfferingCode"
                      p:fieldLabel.render="false"
                      p:uppercaseValue="true"
                      p:required="true"
                      p:readOnly="@{!#isAddLine}">
                    <property name="additionalCssClasses">
                        <list merge="true">
                            <value>ks-uif-quickFinder</value>
                        </list>
                    </property>

                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="4" p:watermarkText="A"/>
                    </property>
                    <property name="quickfinder">
                        <bean parent="Uif-QuickFinder"  p:lightBoxLookup.lookupReturnByScript="true">
                            <property name="dataObjectClassName" value="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper"/>
                            <property name="quickfinderAction">
                                <bean parent="Uif-ActionLink" p:actionLabel="Find an Activity Offering" p:methodToCall="performLookup"/>
                            </property>
                            <property name="autoSearch" value="true"/>
                            <property name="fieldConversions">
                                <map>
                                    <entry key="aoInfo.activityCode" value="activityOfferingCode"/>
                                    <entry key="aoInfo.id" value="aoId"/>
                                </map>
                            </property>
                            <property name="lookupParameters">
                                <map>
                                    <entry key="courseOfferingCode" value="courseOfferingCode"/>
                                    <entry key="editRenderHelper.termId" value="termId"/>
                                </map>
                            </property>
                        </bean>
                    </property>
                </bean>

            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-CoLocatedEnrollmentShared" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <bean id="shared_max_enr" parent="Uif-InputField" p:propertyName="sharedMaxEnrollment" p:label="Shared Maximum Enrollment"
                       p:required="true" p:exclusiveMin="0">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="4" />
                    </property>
                    <property name="additionalCssClasses">
                        <list merge="true">
                            <value>ks-subSection</value>
                        </list>
                    </property>
                </bean>
                <bean parent="Uif-Message"
                      p:messageText="[image src=&quot;@{#ConfigProperties['krad.externalizable.images.url']}/validation/info.png&quot;]  Max enrollment is shared by all co-located activities. [br] Any changes made here will update the other activities on save.">
                    <property name="additionalCssClasses">
                        <list merge="true">
                            <value>ks-informational-message-for-field</value>
                        </list>
                    </property>
                      </bean>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-CoLocatedEnrollmentSeperate" parent="Uif-VerticalBoxSection">
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="Combined Max Enr"/>
                <bean id="enr_shared_table" parent="Uif-TableCollectionSection" p:forceSessionPersistence="true" >
                    <property name="validationMessages.displayMessages" value="false"/>
                    <property name="headerText" value=""/>
                    <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.ColocatedActivity" />
                    <property name="propertyName" value="editRenderHelper.manageSeperateEnrollmentList"/>
                    <property name="renderAddLine" value="false"/>
                    <property name="renderLineActions" value="false"/>
                    <property name="layoutManager.renderSequenceField" value="false"/>
                    <property name="layoutManager.richTable.render" value="false" />
                    <property name="additionalCssClasses">
                        <list merge="true">
                            <value>ks-subSection</value>
                        </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean parent="Uif-DataField" p:propertyName="editRenderHelper.code" p:label="Colocated Activities"/>
                            <bean parent="Uif-InputField" p:propertyName="maxEnrollmentCount" p:label="Max Enr" p:readOnly="@{!#lp.editRenderHelper.allowEnrollmentEdit}"
                                  p:required="true" p:exclusiveMin="0">
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="4" />
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <bean parent="Uif-Message"
                      p:messageText="[image src=&quot;@{#ConfigProperties['krad.externalizable.images.url']}/validation/info.png&quot;]  The max enrollments for all co-located activities will be combined. Any changes may effect room availability.">
                    <property name="additionalCssClasses">
                        <list merge="true">
                            <value>ks-informational-message-for-field</value>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-PersonnelSection" parent="Uif-VerticalBoxSection">
        <property name="headerText" value="Personnel"/>
        <property name="validationMessages.displayMessages" value="false"/>
        <property name="additionalCssClasses">
            <list merge="true">
                <value>ks-section</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean id="ActivityOffering-PersonnelSection" parent="KS-TableCollectionSubSection-BottomAddLine"
                      p:id="ao-personnelgroup" p:layoutManager.style="width: 100%;">
                    <property name="collectionObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.OfferingInstructorWrapper" />
                    <property name="propertyName" value="instructors" />
                    <property name="addBlankLineAction.actionLabel" value="Add Personnel"/>
                    <property name="addBlankLineAction.id" value="ao-personnelgroup-addline"/>
                    <property name="addBlankLineAction.onDocumentReadyScript" value="reDisplayPersonnelSection();"/>
                    <property name="addBlankLineAction.dirtyOnAction" value="true" />
                    <property name="validationMessages.displayMessages" value="false"/>
                    <property name="layoutManager.renderSequenceField" value="false"/>
                    <property name="layoutManager.generateAutoSequence" value="false"/>
                    <property name="layoutManager.separateAddLine" value="false"/>
                    <property name="layoutManager.richTable.disableTableSort" value="true"/>
                    <property name="layoutManager.richTable.render" value="false"/>
                    <property name="layoutManager.applyAlternatingRowStyles" value="false"/>
                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField" p:id="personnel_id" p:propertyName="offeringInstructorInfo.personId"
                                  p:label="ID" p:required="false" p:cssClasses="col_5">
                                <property name="quickfinder">
                                    <bean parent="Uif-QuickFinder">
                                        <property name="dataObjectClassName" value="org.kuali.student.rice.kim.impl.KSPersonImpl"/>
                                        <property name="fieldConversions">
                                            <map>
                                                <entry key="name" value="offeringInstructorInfo.personName"/>
                                                <entry key="principalId" value="offeringInstructorInfo.personId"/>
                                            </map>
                                        </property>
                                        <property name="viewName" value="ks_simplePersonLookup" />
                                        <property name="lightBoxLookup">
                                            <bean parent="Uif-LightBoxPost" p:lookupReturnByScript="true"/>
                                        </property>
                                    </bean>
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:id="personnel_name" p:propertyName="offeringInstructorInfo.personName" p:label="Name" p:required="false"/>
                            <bean parent="Uif-InputField" p:id="personnel_affiliation" p:propertyName="offeringInstructorInfo.typeKey" p:label="Affiliation" p:required="false">
                                <property name="control">
                                    <bean parent="Uif-DropdownControl" p:options-ref="aoInstructors-options" />
                                </property>
                            </bean>
                            <bean parent="Uif-InputField" p:id="personnel_allocation" p:propertyName="sEffort" p:label="Inst.Effort" p:refreshWhenChangedPropertyNames="#lp.offeringInstructorInfo.typeKey" p:required="false" p:inclusiveMax="100">
                                  <property name="validCharactersConstraint">
                                        <bean parent="IntegerPatternConstraint" />
                                  </property>
                                <property name="control">
                                    <bean parent="Uif-TextControl" p:size="10" />
                                </property>
                                <property name="propertyReplacers">
                                  <list>
                                     <bean parent="Uif-ConditionalBeanPropertyReplacer" p:propertyName="exclusiveMin" p:replacement="0" p:condition="#lp.offeringInstructorInfo.typeKey == 'kuali.lpr.type.instructor.main'"/>
                                     <bean parent="Uif-ConditionalBeanPropertyReplacer" p:propertyName="exclusiveMin" p:replacement="-1" p:condition="#lp.offeringInstructorInfo.typeKey == 'kuali.lpr.type.instructor.assistant'"/>
                                  </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                    <property name="lineActions">
                        <list>
                            <bean parent="KS-Uif-Delete-ActionLink" p:id="delete_personnel" p:methodToCall="deleteLine" p:actionLabel="Delete"
                                  p:actionEvent="deleteLine" p:dirtyOnAction="true"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-NoWaitlist" parent="Uif-VerticalBoxSection" p:headerText="Waitlist" p:render="@{!#dp.hasWaitlistCO and !#dp.colocatedAO}">
        <property name="additionalCssClasses">
            <list merge="true">
                <value>ks-section</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="Waitlists are deactivated for the Course Offering"/>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-NoWaitlistColo" parent="Uif-VerticalBoxSection" p:headerText="Waitlist" p:render="@{!#dp.hasWaitlistCO and #dp.colocatedAO}">
        <property name="items">
            <list>
                <bean parent="Uif-Message" p:messageText="Waitlists are deactivated for at least one Course Offering associated with this co-located activity"/>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-Waitlist" parent="Uif-VerticalBoxSection" p:headerText="Waitlist" p:render="@{#dp.hasWaitlistCO}">
        <property name="validationMessages.displayMessages" value="false"/>
        <property name="componentSecurity.viewAuthz" value="true"/>
        <property name="additionalCssClasses">
            <list merge="true">
                <value>ks-section</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-HorizontalBoxSection">
                    <property name="items">
                        <list>
                            <bean id="ActivityOfferingEdit-HasWaitlist" parent="Uif-InputField-LabelRight" p:propertyName="hasWaitlist" p:label="Waitlist Active">
                                <property name="control">
                                    <bean parent="Uif-CheckboxControl" />
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <ref bean="ActivityOfferingEdit-WailtList-Info-Section"/>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingEdit-WailtList-Info-Section" parent="Uif-VerticalBoxSection"
          p:progressiveRender="@{#dp.hasWaitlist}" p:refreshWhenChangedPropertyNames="@{#dp.hasWaitlist}">
        <property name="additionalCssClasses">
            <list merge="true">
                <value>ks-form-panel</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-HorizontalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="Uif-VerticalBoxSection">
                                <property name="items">
                                    <list>
                                        <bean parent="Uif-InputField" p:id="wait_list_type" p:propertyName="waitListType" p:label="Waitlist Type"
                                              p:fieldLabel.renderColon="false" p:labelPlacement="TOP">
                                            <property name="control">
                                                <bean parent="Uif-VerticalRadioControl"/>
                                            </property>
                                            <property name="optionsFinder">
                                                <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.WaitlistTypeOptionsKeyValues" p:addBlankOption="false" />
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-VerticalBoxSection">
                                <property name="items">
                                    <list>
                                        <bean  parent="Uif-MessageField"  p:label="Waitlist Options" p:fieldLabel.renderColon="false"/>
                                        <bean parent="Uif-HorizontalBoxSection">
                                            <property name="items">
                                                <list>
                                                    <bean parent="Uif-InputField-LabelRight" p:propertyName="limitWaitlistSize" p:label="Limit Waitlist Size">
                                                        <property name="control">
                                                            <bean parent="Uif-CheckboxControl" />
                                                        </property>
                                                    </bean>
                                                    <bean  id="limit_waitlist_size_text" parent="Uif-InputField" p:required="true" p:inclusiveMax="999" p:exclusiveMin="0" p:dataType="INTEGER"
                                                           p:propertyName="courseWaitListInfo.maxSize" p:refreshWhenChangedPropertyNames="@{#dp.limitWaitlistSize}">
                                                        <property name="control">
                                                            <bean parent="Uif-TextControl" p:disabled="@{!#dp.limitWaitlistSize}" />
                                                        </property>
                                                    </bean>
                                                </list>
                                            </property>
                                        </bean>
                                        <bean parent="Uif-InputField-LabelRight" p:id="allow-hold-list-checkbox" p:propertyName="courseWaitListInfo.allowHoldUntilEntries" p:label="Allow Hold List">
                                            <property name="control">
                                                <bean parent="Uif-CheckboxControl" />
                                            </property>
                                        </bean>
                                    </list>
                                </property>
