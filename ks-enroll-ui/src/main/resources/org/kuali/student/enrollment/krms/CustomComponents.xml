    <bean id="KRMS-ProgramAndNof" parent="Uif-VerticalFieldGroup">
        <property name="items">
            <list>
                <!-- TODO: Program component -->
                <ref bean="KRMS-NumberOfCourses-ConstantValue" />
            </list>
        </property>
    </bean>

    <bean id="KRMS-ProgramAndCredit" parent="Uif-VerticalFieldGroup">
        <property name="items">
            <list>
                <!-- TODO: Program component -->
