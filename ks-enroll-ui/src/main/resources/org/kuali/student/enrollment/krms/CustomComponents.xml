    </bean>

    <bean id="KRMS-CourseOffering-InputField-Parent" parent="Uif-InputField-LabelTop" p:propertyName="proposition.proposition.termParameter" p:label="Program" p:required="true"/>
    <bean id="KRMS-CourseOffering-InputField" parent="KRMS-CourseOffering-InputField-Parent" />

    <bean id="KRMS-MultiCourseWithGrade-Group" parent="Uif-VerticalFieldGroup">
        <property name="items">
            <list>
                <ref bean="KRMS-MultiCourse-Tree-Section" />
                <ref bean="KRMS-Grade-Tree-Section" />
            </list>
        </property>
    </bean>

    <bean id="KRMS-MultiCourseAndNof-Group" parent="Uif-VerticalFieldGroup">
        <property name="items">
            <list>
                <ref bean="KRMS-MultiCourse-Tree-Section" />
                <ref bean="KRMS-NumberOfCourses-ConstantValue" />
            </list>
        </property>
