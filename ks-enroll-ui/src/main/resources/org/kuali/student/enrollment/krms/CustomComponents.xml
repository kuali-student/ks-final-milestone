                                <property name="quickfinder.viewName" value="KS-KitchenSink-Lookup-TermInfo"/>
                                <property name="quickfinder.fieldConversions">
                                    <map>
                                        <entry key="code" value="proposition.proposition.termParameterList"/>
                                    </map>
                                </property>
                                <property name="quickfinder.quickfinderAction">
                                    <bean parent="Uif-ActionLink">
                                        <property name="methodToCall" value="performLookup"/>
                                        <property name="actionLabel" value="Advanced Search"/>
                                        <property name="style" value="display:block"/>
                                    </bean>
                                </property>
                                <property name="quickfinder.lightBoxLookup">
                                    <bean parent="Uif-LightBoxPost" p:lookupReturnByScript="false"/>
                                </property>-->
                            </bean>
                        </list>
                    </property>
                    <property name="lineActions">
                        <list>
                            <bean parent="Uif-ActionLink">
                                <property name="methodToCall" value="deleteLine"/>
                                <property name="actionLabel" value="delete"/>
                            </bean>
                        </list>
                    </property>
                    <property name="addBlankLineAction">
                        <bean parent="Uif-SecondaryActionButton-Small">
                            <property name="methodToCall" value="addBlankLine"/>
                            <property name="actionEvent" value="addBlankLine"/>
                            <property name="id" value="uif-addBlankLine"/>
                            <property name="actionLabel" value="Add to list"/>
                            <property name="actionImagePlacement" value="LEFT"/>
                            <property name="actionImage">
                                <bean parent="Uif-Image" p:source="../ks-enroll-ksink/images/plus_15px.png"/>
                            </property>
                            <property name="style" value="margin-top:5px;"/>
                        </bean>
                    </property>
                </bean>
            </list>
        </property>
    </bean>
    <bean id="KRMS-CourseSet-AddToList-Section" parent="KRMS-CourseSet-AddToList-Section-Parent" />

    <bean id="KRMS-CourseRange-Section-Parent" parent="Uif-VerticalBoxSection" p:progressiveRender="proposition.multipleCourseType eq '3'">
        <property name="items">
            <list>
                <bean parent="Uif-Label" p:labelText="Course Range" p:renderColon="false" />
                <bean parent="Uif-ActionLink" p:actionLabel="Define Course Range" p:actionScript="showLightboxComponent('myCourseRangeLightBox', {openEffect : 'elastic', helpers:{overlay:{css:{cursor:'pointer', 'background-color':'green'}}}, closeEffect : 'elastic', 'max-width':'55em'});"/>
            </list>
        </property>
    </bean>
    <bean id="KRMS-CourseRange-Section" parent="KRMS-CourseRange-Section-Parent" />

    <bean id="KRMS-CourseRangeLightBox-parent" parent="Uif-VerticalBoxSection">
        <property name="hidden" value="true"/>
        <property name="items">
            <list>
                <bean id="KRMS-SearchBy-Field" parent="Uif-InputField-LabelTop" p:propertyName="proposition.searchByCourseRange">
                    <property name="instructionalText" value="Search By" />
                    <property name="control">
                        <bean parent="Uif-DropdownControl"/>
                    </property>
                    <property name="optionsFinder">
                        <bean class="org.kuali.student.enrollment.class1.krms.keyvalues.CourseRangeValuesFinder"/>
                    </property>
                </bean>

                <bean parent="Uif-VerticalBoxSection" p:instructionalText="Enter one or more fields" p:refreshWhenChangedPropertyNames="proposition.gradeScale">
                    <property name="items">
                        <list>
                            <bean id="KRMS-SubjectCode-Field" parent="Uif-InputField-LabelTop" p:propertyName="proposition.subjectCode" p:label="Subject Code" p:progressiveRender="proposition.searchByCourseRange eq '1'">
                                <property name="control">
                                    <bean parent="Uif-TextControl" />
                                </property>
                            </bean>

                            <bean id="KRMS-CourseNumberRange-Field" parent="Uif-InputField-LabelTop" p:propertyName="proposition.courseNumberRange" p:label="Course Number Range" p:progressiveRender="proposition.searchByCourseRange eq '1'">
                                <property name="control">
                                    <bean parent="Uif-TextControl" />
                                </property>
                            </bean>

                            <bean id="KRMS-Description-Field" parent="Uif-InputField-LabelTop" p:propertyName="proposition.description" p:label="Description" p:progressiveRender="proposition.searchByCourseRange eq '2'">
                                <property name="control">
                                    <bean parent="Uif-TextControl" />
                                </property>
                            </bean>

                            <bean id="KRMS-EffectiveFrom-Field" parent="Uif-InputField-LabelTop" p:propertyName="proposition.effectiveFrom" p:label="Effective From" p:progressiveRender="proposition.searchByCourseRange eq '3'">
                                <property name="control">
                                    <bean parent="Uif-DateControl" />
                                </property>
                            </bean>

                            <bean id="KRMS-EffectiveTo-Field" parent="Uif-InputField-LabelTop" p:propertyName="proposition.effectiveTo" p:label="Effective To" p:progressiveRender="proposition.searchByCourseRange eq '3'">
                                <property name="control">
                                    <bean parent="Uif-DateControl" />
                                </property>
                            </bean>
                        </list>
                    </property>
                    <property name="footer">
                        <bean parent="Uif-FooterBase">
                            <property name="items">
                                <list>
                                    <bean parent="Uif-PrimaryActionButton-Small" p:actionLabel="Add Course Range" p:onClickScript="e.preventDefault();closeLightbox();return false;" />
                                    <bean parent="Uif-ActionLink" p:actionLabel="Cancel"  p:actionScript="jQuery.fancybox.close();" />
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
            </list>
        </property>
    </bean>
    <bean id="KRMS-CourseRangeLightBox" parent="KRMS-CourseRangeLightBox-parent" />



    <bean id="KRMS-GPA-Field-Parent" parent="Uif-InputField-LabelTop" p:propertyName="proposition.proposition.termParameter" p:label="GPA" p:minLength="3" p:maxLength="3" p:required="true">
