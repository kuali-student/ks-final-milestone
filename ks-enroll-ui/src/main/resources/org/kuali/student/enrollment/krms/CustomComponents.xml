    <bean id="KRMS-CampusWithCreditsAndDur" parent="KRMS-CampusAndDur">
        <property name="items">
            <list merge="true">
                <ref bean="KRMS-Credits-ConstantValue" />
            </list>
        </property>
    </bean>

    <bean id="KRMS-CampusWithNofAndDur" parent="KRMS-CampusAndDur">
