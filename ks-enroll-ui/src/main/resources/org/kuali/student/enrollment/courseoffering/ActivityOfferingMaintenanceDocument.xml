                </bean>
                <bean parent="KSInputField" p:label="Days" p:propertyName="newScheduleRequest.days">
                    <property name="constraintText" value="MTWHFSU"/>
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="10"/>
                    </property>
                    <property name="validCharactersConstraint">
						<bean parent="CharsetPatternConstraint" p:validChars="MTWHFSUmtwhfsu" p:labelKey="validation.enroll.ao.scheduleValidChars"/>
