    <!--Note: This is a tab group with link based look and feel. It is not used in any code so far. -->
    <bean id="KS-Uif-TabGroup-LinkTab" parent="Uif-TabGroup">
        <property name="cssClasses">
            <list merge="true">
                <value>ks-uif-tabSection</value>
            </list>
        </property>
    </bean>

    <!--Note by bonnie: Not used in any code -->
    <bean id="KS-Uif-InputField"  parent="KS-Uif-InputField-parent" />
    <bean id="KS-Uif-InputField-parent" abstract="true" parent="Uif-InputField"
          p:fieldLabel.renderColon="false" p:fieldLabel.cssClasses="labelTop"
          p:labelPlacement="TOP" p:fieldLabel.requiredMessagePlacement="LEFT"
          p:instructionalMessage.render="false" >
        <property name="instructionalMessage">
            <bean parent="Uif-InstructionalMessage"/>
        </property>
    </bean>

    <bean id="KS-Uif-InputField-Simple" parent="KS-Uif-InputField-Simple-parent"/>
    <bean id="KS-Uif-InputField-Simple-parent" abstract="true" parent="Uif-InputField" scope="prototype">
        <property name="template" value="/ks-enroll/ftl/ksSimpleDataField.ftl"/>
        <property name="templateName" value="ks_SimpleDataField"/>
        <property name="inquiry">
            <null/>
        </property>
        <property name="help">
            <null/>
        </property>
        <property name="quickfinder">
            <null/>
        </property>
        <property name="validationMessages">
            <null/>
        </property>
        <property name="constraintMessage">
            <null/>
        </property>
        <property name="instructionalMessage">
            <null/>
        </property>
    </bean>

    <bean id="KS-Uif-DataField-Simple" parent="KS-Uif-DataField-Simple-parentBean"/>
    <bean id="KS-Uif-DataField-Simple-parentBean" abstract="true" parent="Uif-DataField" scope="prototype">
        <property name="template" value="/ks-enroll/ftl/ksSimpleDataField.ftl"/>
        <property name="templateName" value="ks_SimpleDataField"/>
        <property name="inquiry">
            <null/>
        </property>
        <property name="help">
            <null/>
        </property>
    </bean>

    <!--Note by bonnie: Not used in any code -->
    <bean id="KS-Uif-Link-DisplayInLightBox" parent="Uif-Link">
        <property name="lightBox">
            <bean parent="Uif-LightBox"/>
        </property>
        <property name="target" value="_blank"/>
    </bean>

    <!--Note by bonnie: Not used in any code -->
    <bean id="KS-Uif-Link-Edit" parent="KS-Uif-Link-DisplayInLightBox" p:order="10">
        <property name="linkText" value="Edit"/>
        <property name="finalizeMethodToCall" value="getMaintenanceActionLink"/>
        <property name="finalizeMethodAdditionalArguments">
            <list>
                <value>maintenanceEdit</value>
            </list>
        </property>
        <property name="render" value="@{#ViewHelper.allowsMaintenanceEditAction(#line)}"/>
    </bean>

    <!--p:onDocumentReadyScript="decorateToolbar();-->

    <bean id="KS-Uif-HorizontalBoxSection-ToolbarSection" parent="KS-Uif-HorizontalBoxSection-ToolbarSection-parent"/>
    <bean id="KS-Uif-HorizontalBoxSection-ToolbarSection-parent" abstract="true" parent="Uif-HorizontalBoxSection" scope="prototype">
        <property name="cssClasses">
            <list merge="true">
                <value>ks-toolbar</value>
            </list>
        </property>
    </bean>

    <bean id="KS-Uif-HorizontalBoxGroup-ToolbarGroup" parent="KS-Uif-HorizontalBoxGroup-ToolbarGroup-parent"/>
    <bean id="KS-Uif-HorizontalBoxGroup-ToolbarGroup-parent" parent="Uif-HorizontalBoxGroup" class="org.kuali.student.enrollment.uif.container.KSToolbarGroup" abstract="true" scope="prototype">
        <property name="template" value="/ks-enroll/ftl/ksToolbarGroup.ftl"/>
        <property name="templateName" value="ks_ToolbarGroup"/>
        <property name="cssClasses">
            <list merge="true">
                <value>ks-toolbar-group-padded</value>
            </list>
        </property>
    </bean>

    <bean id="KS-Uif-ToolBar-Button" parent="Uif-SecondaryActionButton" p:disabled="true" abstract="true">
