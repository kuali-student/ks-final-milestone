                    <bean parent="Uif-HorizontalFieldGroup"/>
                </property>
            </bean>
        </property>
        <property name="items">
            <list/>
        </property>
    </bean>

    <!--Note by bonnie: Not used in any code -->
    <bean id="KS-Uif-StackedCollectionSection-HorizontalSubCollectionFieldGroupWithDisclosure" parent="KS-Uif-StackedCollectionSection-HorizontalSubCollectionFieldGroup">
        <property name="disclosure.render" value="true"/>
        <!-- TODO: check with UX, do we need to set disclosure as KS-Uif-Disclosure by default? -->
        <!--<property name="disclosure">-->
            <!--<bean parent="KS-Uif-Disclosure"/>-->
        <!--</property>-->
