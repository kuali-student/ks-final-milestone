    <bean id="KS-Uif-StackedCollectionSubSection-HorizontalSubCollectionFieldGroup" parent="Uif-StackedCollectionSubSection">
        <property name="layoutManager">
            <bean parent="Uif-StackedCollectionLayout-WithBoxItems">
                <property name="subCollectionFieldGroupPrototype">
                    <bean parent="Uif-HorizontalFieldGroup"/>
                </property>
            </bean>
        </property>
        <property name="items">
            <list/>
        </property>
    </bean>

    <bean id="KS-Uif-StackedCollectionSubSection-HorizontalSubCollectionFieldGroupWithDisclosure" parent="KS-Uif-StackedCollectionSubSection-HorizontalSubCollectionFieldGroup">
        <property name="disclosure.render" value="true"/>
        <!-- TODO: check with UX, do we need to set disclosure as KS-Uif-Disclosure by default? -->
        <!--<property name="disclosure">-->
        <!--<bean parent="KS-Uif-Disclosure"/>-->
        <!--</property>-->
