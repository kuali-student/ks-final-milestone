<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>org.kuali.student.lum</groupId>
	<artifactId>ks-lum-rice</artifactId>
	<version>1.0.0-rc2</version>
	<packaging>jar</packaging>
	<name>KS LUM Rice</name>
	<description>KS Learning Unit Management-rice</description>

	<parent>
		<groupId>org.kuali.student.lum</groupId>
		<artifactId>ks-lum</artifactId>
		<version>1.0.0-rc2</version>
	</parent>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<skipTests>true</skipTests>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		<dependency>
			<groupId>org.kuali.student.core</groupId>
			<artifactId>ks-rice-lessdep</artifactId>
			<version>${ks-core.version}</version>
			<type>pom</type>
			<scope>provided</scope>
		</dependency>			
		<dependency>
			<groupId>org.kuali.student.core</groupId>
			<artifactId>ks-core-rice</artifactId>
			<version>${ks-core.version}</version>
		</dependency>
		<dependency>
			<artifactId>servlet-api</artifactId>
			<groupId>javax.servlet</groupId>
		</dependency>
		<dependency>
			<groupId>org.kuali.student.lum</groupId>
			<artifactId>ks-lum-api</artifactId>
			<version>1.0.0-rc2</version>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-orm</artifactId>
		</dependency>
		<dependency>
			<groupId>org.springframework.security</groupId>
			<artifactId>spring-security-core</artifactId>
		</dependency>
	</dependencies>

</project>