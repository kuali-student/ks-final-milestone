<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Controller Class: CourseController.java View Helper Class: CourseViewHelperServiceImpl.java UX : http://ux.ks.kuali.org.s3.amazonaws.com/wireframes/ENR/Scheduling_Interface/v3/start.html 
        This view has couple of pages (edit and copy) with all the beans defined at 1. CourseViewCourseInfoPage.xml -->

    <bean id="KS-CourseView" parent="KS-Uif-MaintenanceView">
        <property name="defaultBindingObjectPath" value="document.newMaintainableObject" />
        <property name="formClass" value="org.kuali.student.cm.course.form.CreateCourseForm"/>
        <property name="dataObjectClassName" value="org.kuali.student.r2.lum.course.dto.CourseInfo"/>
        <!-- New Course header requires static string therefore below string construction is not required.
        commenting below code in case required in future.
         @{!#empty(document.newMaintainableObject.proposal.name) ? document.newMaintainableObject.proposal.name + ' (Proposal)': 'New Course (Proposal)'} -->
        <!--<property name="headerText" value="@{(document.newMaintainableObject.proposal != null and !#empty(document.newMaintainableObject.proposal.name)) ? document.newMaintainableObject.proposal.name + ' (Proposal)': 'New Course (Proposal)'}" />-->
        <property name="header.render" value="false" />
        <property name="entryPageId" value="KS-CourseView-createCourseInitialPage" />
        <property name="singlePageView" value="false"/><!-- Override the default to allow for navigation -->
        <property name="viewHelperServiceClass" value="org.kuali.student.cm.course.service.impl.CourseInfoMaintainableImpl" />
        <property name="breadcrumbOptions.homewardPathBreadcrumbs">
            <list>
                <ref bean="KS-HomewardPathBreadcrumbs-Home"/>
                <ref bean="KS-HomewardPathBreadcrumbs-CMKRAD"/>
            </list>
        </property>
        <property name="stateMapping">
            <bean parent="WorkflowStateMapping">
                <property name="customClientSideValidationStates">
                    <map>
                        <entry key="I" value="A"/>
                    </map>
                </property>
            </bean>
        </property>
        <property name="navigation">
            <ref bean="KS-CourseNavigation-Menu" />
        </property>
        <property name="onDocumentReadyScript" value="onCourseLoad();"/>
        <property name="dialogs">
            <list>
                <ref bean="commentsLightBox" />
                <ref bean="decisionsDialog" />
                <!-- <bean id="compareRuleLightBox" parent="KS-KRMS-CompareRule">
                    <property name="headerText" value="Compare Catalog and Course Offering Rules"/>
                </bean> -->
                <bean id="courseRangeLookup" parent="KRMS-CourseRangeResult-Lookup-CM" p:fieldBindingObjectPath="document.newMaintainableObject.courseRuleManagementWrapper"/>
                <bean id="warningMessagedYesNo" parent="KRMS-WarningMessagedYesNo" p:fieldBindingObjectPath="document.newMaintainableObject.courseRuleManagementWrapper"/>
            </list>
        </property>
        <property name="items">
            <list>
                <ref bean="KS-CourseView-createCourseInitialPage" />
                <ref bean="KS-CourseView-CoursePage"/>
                <ref bean="KS-RuleMaintenance-Page-Parent" />
            </list>
        </property>
        <property name="additionalScriptFiles">
            <list>
                <value>ks-cm-krad/scripts/jquery.jqEasyCharCounter.custom.js</value>
                <value>ks-cm-krad/scripts/common.js</value>
                <value>themes/ksboot/scripts/ruleMaintenance.js</value>
                <value>themes/ksboot/scripts/toolbar.js</value>
            </list>
        </property>
        <property name="additionalCssFiles">
            <list>
                <value>ks-cm-krad/css/cm.css</value>
                <value>themes/ksboot/stylesheets/krms.css</value>
            </list>
        </property>
    </bean>
    
    <bean id="CourseInfoMaintenanceDocument" parent="uifMaintenanceDocumentEntry">
        <property name="dataObjectClass" value="org.kuali.student.r2.lum.course.dto.CourseInfo" />
        <property name="maintainableClass" value="org.kuali.student.cm.course.service.impl.CourseInfoMaintainableImpl"/>
        <property name="documentTypeName" value="kuali.proposal.type.course.create"/>
        <property name="documentAuthorizerClass" value="org.kuali.student.cm.course.auth.CMMaintenanceDocumentAuthorizer"/>
        <property name="lockingKeys">
            <list>
                <value>id</value>
            </list>
        </property>
    </bean>  

    <bean id="KS-CourseView-BasePage" parent="Uif-MaintenancePage" p:disclosure.render="false"/>

    <bean id="KS-CourseView-CoursePage" parent="Uif-MaintenancePage" p:disclosure.render="false">
        <property name="header" ref="KS-CourseView-Header" />
        <property name="footer">
            <bean parent="KS-CourseView-Footer-Fred"/>
        </property>
        <property name="propertyReplacers">
          <list>
            <bean parent="Uif-ConditionalBeanPropertyReplacer" p:propertyName="footer" p:condition="#hasPerm('KS-SYS', 'Create Course By Admin Proposal')">
              <property name="replacement">
                  <bean parent="KS-CourseView-Footer-Admin"/>
              </property>
            </bean>
          </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-TabGroup" p:id="course_tabs" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal') eq false}">
                    <property name="items">
                        <list>
                            <ref bean="KS-CourseView-CourseInfoPage-Widgets"/>
                            <ref bean="KS-CourseView-GovernancePage-Widgets"/>
                            <ref bean="KS-CourseView-Logistics-Widgets"/>
                            <ref bean="KS-CourseView-LearningObjectivesPage-Widgets" />
                            <ref bean="KS-CourseView-CourseRequisites-Widgets"/>
                            <ref bean="KS-CourseView-ActiveDatesPage-Widgets"/>
                            <ref bean="KS-CourseView-Financials"/>
                            <ref bean="KS-CourseView-AuthorsAndCollaboratorsPage-Widgets"/>
                            <ref bean="KS-CourseView-SupportingDocumentsPage-Widgets" />
                            <ref bean="KS-CourseView-ReviewProposalPage-Widgets"/>
                        </list>
                    </property>
                    <property name="tabsWidget.templateOptionsJSString">
                        <value>
                            { selected: @{selectedTabIndex} }
                        </value>
                    </property>
                    <property name="tabsWidget.position" value="LEFT"/>
                </bean>
                <bean id="courseInfoSection" parent="KS-CourseView-CourseInfoPage-Widgets"  p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}"/>
                <bean id="governanceSetion" parent="KS-CourseView-GovernancePage-Widgets" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}"/>
                <bean id="logisticsSection" parent="KS-CourseView-Logistics-Widgets" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}"/>
                <bean id="learningObjectivesSection" parent="KS-CourseView-LearningObjectivesPage-Widgets" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}"/>
                <bean id="requisitesSection" parent="KS-CourseView-CourseRequisites-Widgets" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}"/>
                <bean id="activeDatesSection" parent="KS-CourseView-ActiveDatesPage-Widgets" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}"/>
                <bean id="financialsSection" parent="KS-CourseView-Financials" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}"/>
                <bean id="authorsAndCollaboratorsSection" parent="KS-CourseView-AuthorsAndCollaboratorsPage-Widgets" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}"/>
                <bean id="supportingDocumentsSection" parent="KS-CourseView-SupportingDocumentsPage-Widgets" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}"/>
                <bean id="reviewProposalSection" parent="KS-CourseView-ReviewProposalPage-Widgets" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}"/>

            </list>
        </property>
    </bean>

    <bean id="KS-CourseView-ReviewLink" parent="Uif-VerticalBoxGroup">
        <property name="cssClasses">
            <list merge="true">
                <value>ks-header</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-PrimaryActionButton" 
                      p:ajaxSubmit="false" 
                      p:methodToCall="navigate&amp;pageId=KS-CourseView-CourseInfoPage" 
                      p:actionLabel="Edit"/>
            </list> 
        </property>
    </bean>

    <bean id="KS-CourseView-SectionHeader" parent="Uif-HeaderTwo">
        <property name="cssClasses">
            <list merge="true">
                <value>ks-header</value>
            </list>
        </property>
    </bean>
          
    <bean id="KS-CourseView-Header" parent="Uif-HeaderTwo">
        <property name="lowerGroup">
            <ref bean="Uif-HeaderLowerGroup"/>
        </property>
        <property name="upperGroup">
            <bean parent="Uif-VerticalBoxGroup">
                <property name="items">
                    <list>
                        <bean parent="Uif-VerticalBoxGroup">
                            <property name="cssClasses">
                                <list merge="true">
                                    <value>ks-cm-page-header</value>
                                </list>
                            </property>
                            <property name="items">
                                <list>
                                    <bean parent="Uif-HeaderTwo">
                                        <property name="headerText" value="@{!#empty(document.newMaintainableObject.proposal.name) ? document.newMaintainableObject.proposal.name + ' (Proposal)': 'New Course (Proposal)'}" />
                                        <property name="cssClasses">
                                            <list merge="true">
                                                <value>ks-header</value>
                                            </list>
                                        </property>
                                    </bean>
                                    <bean parent="Uif-LinkGroup" p:linkSeparator="|">
                                        <property name="items">
                                            <list>
                                                <bean parent="Uif-DataField" p:label="Proposal Status"
                                                      p:shortLabel="Status" p:labelPlacement="LEFT" p:propertyName="course.stateKey" />
                                                <bean parent="Uif-ActionLink" p:actionLabel="Comments"
                                                      p:methodToCall="showComment" />
                                                <bean parent="Uif-ActionLink" p:actionLabel="Decisions"
                                                      p:methodToCall="showDecisions" />
                                                <bean parent="Uif-Space" />
                                                <bean parent="Uif-DataField" p:label="Last Updated"
                                                      p:shortLabel="Updated" p:labelPlacement="LEFT" p:propertyName="lastUpdated" />
                                            </list>
                                        </property>
                                    </bean>
                                </list>
                            </property>
                        </bean>
                        <bean parent="Uif-VerticalBoxGroup" p:render="@{#hasPerm('KS-SYS', 'Create Course By Admin Proposal')}">
                            <property name="items">
                                <list>
                                    <bean parent="Uif-Message">
                                        <property name="cssClasses">
                                            <list merge="true">
                                                <value>uif-validationMessages</value>
                                                <value>uif-groupValidationMessages</value> 
                                                <value>uif-pageValidationMessages</value>
                                                <value>uif-pageValidationMessages-warning</value>
                                            </list>
                                        </property>
                                        <property name="messageText"
                                                  value="Showing required and optional fields. Show only required fields" />
                                    </bean>
                                </list>
                            </property>
                        </bean>
                    </list>
                </property>
            </bean>
        </property>
    </bean>
    
    <bean id="KS-CourseView-Footer-Admin" parent="Uif-FooterBase">
        <property name="items">
            <list>
                <bean parent="Uif-PrimaryActionButton"
                      p:ajaxSubmit="false"
                      p:methodToCall="performRouteReport"
                      p:actionLabel="Route Report"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_PERFORM_ROUTE_REPORT]}"/>
                <bean parent="Uif-PrimaryActionButton"
                      p:ajaxSubmit="false"
                      p:methodToCall="sendAdHocRequests"
                      p:actionLabel="Send Adhoc"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_SEND_ADHOC_REQUESTS]}"/>
                <bean parent="Uif-PrimaryActionButton"
                      p:ajaxSubmit="false" 
                      p:methodToCall="route" 
                      p:actionLabel="Submit" 
                      p:performClientSideValidation="true"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE] and #form.pageId eq 'KS-CourseView-ReviewProposalPage'}"/>
                <bean parent="Uif-SaveAction" 
                      p:ajaxSubmit="false" 
                      p:methodToCall="saveProposal"
                      p:actionLabel="Save and Continue"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_SAVE]}">
                      <property name="actionParameters">
                          <map>
                            <entry key="displayPage" value="NEXT"/>
                          </map>
                      </property>
                </bean>
                <bean parent="Uif-PrimaryActionButton"
                      p:ajaxSubmit="false" 
                      p:methodToCall="reload" 
                      p:actionLabel="Reload"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_RELOAD]}"/>
                <bean parent="Uif-PrimaryActionButton"
                      p:ajaxSubmit="false"
                      p:methodToCall="blanketApprove"
                      p:actionLabel="Approve and Activate"
                      p:performClientSideValidation="true"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_BLANKET_APPROVE]}"/>
                <bean parent="Uif-PrimaryActionButton"
                      p:ajaxSubmit="false" 
                      p:methodToCall="approve" 
                      p:actionLabel="Approve" 
                      p:performClientSideValidation="true"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_APPROVE]}"/>
                <bean parent="Uif-PrimaryActionButton" 
                      p:ajaxSubmit="false" 
                      p:methodToCall="disapprove" 
                      p:actionLabel="Disapprove"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_DISAPPROVE]}"/>
                <bean parent="Uif-PrimaryActionButton" 
                      p:ajaxSubmit="false" 
                      p:methodToCall="fyi" 
                      p:actionLabel="FYI"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_FYI]}"/>
                <bean parent="Uif-PrimaryActionButton" 
                      p:ajaxSubmit="false" 
                      p:methodToCall="acknowledge" 
                      p:actionLabel="Acknowledge"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ACKNOWLEDGE]}"/>
                <bean parent="Uif-BackAction" 
                      p:ajaxSubmit="false" 
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CLOSE]}"/>
                <bean parent="Uif-CancelAction" 
                      p:ajaxSubmit="false" 
                      p:methodToCall="cancel" 
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CANCEL]}"/>
                <bean parent="Uif-ActionLink" 
                      p:ajaxSubmit="false"
                      p:methodToCall="copy" 
                      p:actionLabel="copy"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_COPY]}"/>
            </list>
        </property>
    </bean>

    <bean id="KS-CourseView-Footer-Fred" parent="Uif-FooterBase" p:layoutManager.style="float:none;">
        <property name="items">
            <list>
                <bean parent="Uif-HorizontalBoxGroup" p:style="float:left;">
                    <property name="items">
                        <list>
                            <bean parent="Uif-SecondaryActionButton"
                                  p:ajaxSubmit="false"
                                  p:methodToCall="previousPage"
                                  p:actionLabel="&lt; Back"
                                  p:performClientSideValidation="true"
                                  p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_SAVE] and #form.pageId ne 'KS-CourseView-CourseInfoPage'}"/>
                        </list>
                    </property>
                </bean>
                <bean parent="Uif-HorizontalBoxGroup">
                    <property name="items">
                        <list>
                            <bean parent="Uif-SaveAction"
                                  p:ajaxSubmit="false"
                                  p:methodToCall="saveProposal"
                                  p:actionLabel="Save and Continue"
                                  p:performClientSideValidation="true"
                                  p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_SAVE]}">
                                  <property name="actionParameters">
                                      <map>
                                        <entry key="displayPage" value="NEXT"/>
                                      </map>
                                  </property>
                            </bean>
                            <bean parent="Uif-SecondaryActionButton"
                                  p:ajaxSubmit="false"
                                  p:methodToCall="saveProposal"
                                  p:actionLabel="Save Progress"
                                  p:performClientSideValidation="true"
                                  p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_SAVE]}">
                            </bean>
                            <bean parent="Uif-ActionLink"
                                  p:ajaxSubmit="false"
                                  p:methodToCall="back"
                                  p:actionLabel="Cancel"
                                  p:style="margin-top:5px;"
                                  p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CLOSE]}"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

