    <bean id="SupportingDocuments-Review-section" parent="ReviewProposal-VerticalBoxSection">
        <property name="headerText" value="Supporting Documents"/>
        <property name="header.rightGroup.items">
            <list>
                <bean p:id="Governance-Review-Edit-link" parent="Uif-ActionLink-Course-Proposal-Review-Page-Edit-Link">
