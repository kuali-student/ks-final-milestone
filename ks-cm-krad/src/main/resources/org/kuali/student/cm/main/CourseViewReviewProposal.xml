<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="KS-CourseView-ReviewProposalPage" parent="Uif-MaintenancePage" p:style="margin-left: -18px;"
          p:headerText="Review Proposal"
          p:disclosure.render="false">

        <!-- Don't show validation messages at the top of the page. Instead we want the yellow bar. -->
        <property name="validationMessages.displayMessages" value="false"/>

        <!-- Initialize the page -->
        <property name="onDocumentReadyScript" value="onProposalReviewLoad();"/>

        <property name="breadcrumbOptions.homewardPathBreadcrumbs">
            <list>
                <ref bean="KS-HomewardPathBreadcrumbs-Home"/>
                <ref bean="KS-HomewardPathBreadcrumbs-CurriculumManagement"/>
                <ref bean="KS-HomewardPathBreadcrumbs-NewCourse"/>
            </list>
        </property>
        <property name="breadcrumbOptions.breadcrumbOverrides">
            <list>
                <ref bean="KS-HomewardPathBreadcrumbs-ReviewProposal-Page"/>
            </list>
        </property>
        <property name="header">
            <bean id="KS-CourseView-ReviewProposal-Header" parent="Uif-HeaderOne" p:style="margin-bottom: 20px;"
                  p:render="true"/>
        </property>
        <property name="items">
            <list>
                <bean id="KS-CourseView-ReviewProposal-Section" parent="KS-CourseView-VerticalBoxSection"
                      p:headerText="">
                    <property name="items">
                        <list>
                            <ref bean="KS-CourseView-ReviewErrorMessageLink"/>
                            <ref bean="CourseInfo-Review-section"/>
                            <ref bean="Governance-Review-section"/>
                            <ref bean="CourseLogistics-Review-section"/>
                            <ref bean="LearningObjectives-Review-section"/>
                            <ref bean="CourseRequisitesPage-Review-section"/>
                            <ref bean="ActiveDates-Review-section"/>
                            <ref bean="Financials-Review-section"/>
                            <ref bean="AuthorsAndCollaborators-Review-section"/>
                            <ref bean="SupportingDocuments-Review-section"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="footer">
            <bean parent="KS-CourseView-ReviewProposal-Footer"/>
        </property>
    </bean>

    <bean id="CM-ReviewProposal-GridGroup" parent="Uif-GridGroup" p:layoutManager.numberOfColumns="4" abstract="true">
        <property name="cssClasses">
            <list merge="true">
                <value>review_proposal_details-grid</value>
                <value>ks-no-margins</value>
                <value>ks-subSection</value>
            </list>
        </property>
    </bean>

    <bean id="KS-CourseView-ReviewErrorMessageLink" parent="Uif-VerticalBoxGroup"
          p:render="@{#dataObject.missingRequiredFields}">
        <property name="items">
            <list>
                <bean id="KS-CourseView-Admin-Message-TreeExpandCollapse" parent="Uif-HorizontalFieldGroup"
                      p:showLabel="false"
                      p:additionalCssClasses="uif-validationMessages,uif-groupValidationMessages,uif-pageValidationMessages-warning,ks-subSection">
                    <property name="items">
                        <list>
                            <bean parent="Uif-Message" p:id="ReviewProposal-Error-Message"
                                  p:messageText="This proposal is incomplete."
                                  p:style="margin-right: 0pt; margin-top: 5px;">
                            </bean>
                            <bean parent="KS-Uif-ActionLink" p:id="ReviewProposal-Error-Message-expand-optional-link"
                                  p:actionScript="showHideReviewProposalErrorFields('KS-CourseView-ReviewProposal-Section');"
                                  p:actionLabel="Hide missing-fields indicator."
                                  p:style="margin-right: 15pt; margin-top: 0px;">
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <!-- A read-only text field -->
    <bean id="CM-ReadOnlyInput" parent="KS-Uif-InputField" p:colSpan="3" p:escapeHtmlInPropertyValue="false"
          class="org.kuali.student.cm.uif.element.InputFieldWithNonClobberableFieldLabel" abstract="true">
        <property name="fieldLabel.renderColon" value="false"/>
        <property name="cssClasses">
            <list merge="true">
                <value>review_readonly</value>
            </list>
        </property>
        <property name="control">
            <bean parent="Uif-LargeTextAreaControl" p:readOnly="true">
                <property name="cssClasses">
                    <list merge="true">
                        <!-- This is really just used by the jQuery selector. -->
                        <value>review_readonly_control</value>
                    </list>
                </property>
            </bean>
        </property>
        <!-- Rice 2.4 Upgrade: we can no longer customize the location of the required message
         TODO: KSCM-2000 Required Message Changes
        <property name="fieldLabel.requiredMessagePlacement" value="LEFT"/>
        -->
    </bean>

    <!-- The parent all of the vertical box sections -->
    <bean id="ReviewProposal-VerticalBoxSection" parent="Uif-VerticalBoxSection" abstract="true">
        <property name="validationMessages.displayMessages" value="false"/>
        <property name="cssClasses">
            <list merge="true">
                <value>review_proposal-section</value>
            </list>
        </property>
        <property name="header">
            <bean parent="Uif-HeaderTwo">
                <property name="cssClasses">
                    <list merge="true">
                        <value>ks-dottedHeaderActions</value>
                        <value>cm_dottedHeaderActions</value>
                    </list>
                </property>
                <property name="rightGroup">
                    <bean parent="KS-CourseView-ReviewLink"/>
                </property>
            </bean>
        </property>
    </bean>

    <bean id="CourseInfo-Review-section" parent="ReviewProposal-VerticalBoxSection">
        <property name="headerText" value="Course Information"/>
        <property name="header.rightGroup.items">
            <list>
                <bean p:id="CourseInfo-Review-Edit-link" parent="Uif-ActionLink-Course-Proposal-Review-Page-Edit-Link">
                    <property name="actionParameters">
                        <map>
                            <entry key="displaySection" value="KS-CourseView-CourseInfo-Section"/>
                        </map>
                    </property>
                </bean>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="CM-ReviewProposal-GridGroup">
                    <property name="items">
                        <list>
                            <bean parent="CM-ReadOnlyInput" p:label="Proposal Title" id="proposalInfo_name"
                                  p:propertyName="proposalInfo.name"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Course Title" id="courseInfo_courseTitle"
                                  p:propertyName="courseInfo.courseTitle"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Transcript Course Title"
                                  p:propertyName="courseInfo.transcriptTitle"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Subject Code" id="courseInfo_subjectArea"
                                  p:propertyName="courseInfo.subjectArea"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Course Number" id="courseInfo_courseNumberSuffix"
                                  p:propertyName="courseInfo.courseNumberSuffix"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Cross Listed Courses"
                                  id="courseSection_crossListings"
                                  p:propertyName="reviewProposalDisplay.courseSection.crossListingsAsString"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Jointly Offered Courses"
                                  id="courseSection_jointly_offered_courses"
                                  p:propertyName="reviewProposalDisplay.courseSection.jointlyOfferedCoursesAsString"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Version Codes" id="courseSection_Version_Codes"
                                  p:propertyName="reviewProposalDisplay.courseSection.variationsAsString"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Instructor(s)" id="courseSection_instructors"
                                  p:propertyName="reviewProposalDisplay.courseSection.instructorsAsString"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Description" id="courseInfo_descr"
                                  p:propertyName="courseInfo.descr.plain"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Proposal Rationale" id="proposalInfo_rationale"
                                  p:propertyName="proposalInfo.rationale.plain"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="Governance-Review-section" parent="ReviewProposal-VerticalBoxSection">
        <property name="headerText" value="Governance"/>
        <property name="header.rightGroup.items">
            <list>
                <bean p:id="Governance-Review-Edit-link" parent="Uif-ActionLink-Course-Proposal-Review-Page-Edit-Link">
                    <property name="actionParameters">
                        <map>
                            <entry key="displaySection" value="KS-CourseView-Governance-Section"/>
                        </map>
                    </property>
                </bean>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="CM-ReviewProposal-GridGroup">
                    <property name="items">
                        <list>
                            <bean parent="CM-ReadOnlyInput" p:label="Campus Locations"
                                  id="governanceSection_campusLocations"
                                  p:propertyName="reviewProposalDisplay.governanceSection.campusLocationsAsString"/>

                            <bean parent="CM-ReadOnlyInput" p:label="Curriculum Oversight"
                                  id="governanceSection_curriculumOversightAsString"
                                  p:propertyName="reviewProposalDisplay.governanceSection.curriculumOversightAsString">
                                <!-- Make required for state enroute -->
                                <property name="simpleConstraint">
                                    <bean parent="RequiredConstraint">
                                        <property name="states">
                                            <list>
                                                <value>R+</value>
                                            </list>
                                        </property>
                                    </bean>
                                </property>
                            </bean>
                            <bean parent="CM-ReadOnlyInput" p:label="Administering Organization"
                                  id="governanceSection_administeringOrganization"
                                  p:propertyName="reviewProposalDisplay.governanceSection.administeringOrganizationAsString"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="CourseLogistics-Review-section" parent="ReviewProposal-VerticalBoxSection">
        <property name="headerText" value="Course Logistics"/>
        <property name="header.rightGroup.items">
            <list>
                <bean p:id="Governance-Review-Edit-link" parent="Uif-ActionLink-Course-Proposal-Review-Page-Edit-Link">
                    <property name="actionParameters">
                        <map>
                            <entry key="displaySection" value="KS-CourseView-Logistics-Section"/>
                        </map>
                    </property>
                </bean>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="CM-ReviewProposal-GridGroup">
                    <property name="items">
                        <list>
                            <bean parent="CM-ReadOnlyInput" p:label="Terms" id="courseLogisticsSection_terms"
                                  p:propertyName="reviewProposalDisplay.courseLogisticsSection.termsAsString"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Duration"
                                  id="courseLogisticsSection_atpDurationType"
                                  p:propertyName="reviewProposalDisplay.courseLogisticsSection.durationDisplay"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Assessment Scale"
                                  id="courseLogisticsSection_gradingOptions"
                                  p:propertyName="reviewProposalDisplay.courseLogisticsSection.gradingOptionsAsString">
                                <!-- Make required for state enroute -->
                                <property name="simpleConstraint">
                                    <bean parent="RequiredConstraint">
                                        <property name="states">
                                            <list>
                                                <value>R+</value>
                                            </list>
                                        </property>
                                    </bean>
                                </property>
                            </bean>

                            <bean parent="CM-ReadOnlyInput" p:label="Audit" id="courseLogisticsSection_audit"
                                  p:propertyName="reviewProposalDisplay.courseLogisticsSection.audit">
                                <property name="control.cssClasses">
                                    <list merge="true">
                                        <value>ks-review-captialize</value>
                                        <value>review_readonly_control</value>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="CM-ReadOnlyInput" p:label="Pass Fail Transcript Grade"
                                  id="courseLogisticsSection_passFail"
                                  p:propertyName="reviewProposalDisplay.courseLogisticsSection.passFail">
                                <property name="control.cssClasses">
                                    <list merge="true">
                                        <value>ks-review-captialize</value>
                                        <value>review_readonly_control</value>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="CM-ReadOnlyInput" p:label="Final Exam Status"
                                  id="courseLogisticsSection_finalExamStatus"
                                  p:propertyName="reviewProposalDisplay.courseLogisticsSection.finalExamStatus"/>
                            <bean parent="CM-ReadOnlyInput" p:label="Final Exam Rationale"
                                  id="courseLogisticsSection_finalExamStatusRationale"
                                  p:propertyName="reviewProposalDisplay.courseLogisticsSection.finalExamStatusRationale"/>

                            <!-- This bean gets rendered if the outComes collection has items. Otherwise, the one below does. -->
                            <bean parent="Uif-VerticalFieldGroup" p:label="Outcomes" p:colSpan="3"
                                  p:fieldLabel.renderColon="false"
                                  p:render="@{!#emptyList(#dataObject.reviewProposalDisplay.courseLogisticsSection.outcomes)}">
                                <property name="items">
                                    <list>
                                        <bean id="course_review_outcome_details" parent="Uif-StackedCollectionSection"
                                              p:propertyName="reviewProposalDisplay.courseLogisticsSection.outcomes"
                                              p:collectionObjectClass="org.kuali.student.cm.course.form.OutcomeReviewSection">
                                            <property name="layoutManager.lineGroupPrototype.header.headerText"
                                                      value="Outcome @{(#index + 1)}"/>
                                            <property name="layoutManager.lineGroupPrototype.header.style"
                                                      value="border-bottom : 1px solid black;"/>
                                            <property name="renderLineActions" value="false"/>
                                            <property name="renderAddLine" value="false"/>
                                            <property name="items">
                                                <list>
                                                    <bean parent="CM-ReadOnlyInput" p:label="Type"
                                                          p:propertyName="outComeType" p:colSpan="1"
                                                          p:additionalCssClasses="ks-stackedcollection-table-outcome"/>
                                                    <bean parent="CM-ReadOnlyInput" p:label="Credit Value"
                                                          p:fieldLabel.style="white-space : nowrap; margin-left : -4px;"
                                                          p:propertyName="outComeValue" p:colSpan="1"
                                                          p:additionalCssClasses="ks-stackedcollection-table-outcome"/>
                                                </list>
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>
                            <!-- This bean gets rendered if the outComes collection is empty. Otherwise, the one above does. -->
                            <bean parent="CM-ReadOnlyInput" p:label="Outcomes"
                                  p:propertyName="reviewProposalDisplay.courseLogisticsSection.emptyStringOutcomes"
                                  p:render="@{#emptyList(#dataObject.reviewProposalDisplay.courseLogisticsSection.outcomes)}">
                                <!-- Make required for state enroute  -->
                                <property name="simpleConstraint">
                                    <bean parent="RequiredConstraint">
                                        <property name="states">
                                            <list>
                                                <value>R+</value>
                                            </list>
                                        </property>
                                    </bean>
                                </property>
                            </bean>

                            <!-- This bean gets rendered if the formats collection has items. Otherwise, the one below does. -->
                            <bean parent="Uif-VerticalFieldGroup" p:label="Course Formats" p:colSpan="3"
                                  p:fieldLabel.renderColon="false"
                                  p:render="@{!#emptyList(#dataObject.reviewProposalDisplay.courseLogisticsSection.formatInfoWrappers)}">
                                <property name="items">
                                    <list>
                                        <bean id="course_review_format_details" parent="Uif-StackedCollectionSection"
                                              p:propertyName="reviewProposalDisplay.courseLogisticsSection.formatInfoWrappers"
                                              p:collectionObjectClass="org.kuali.student.cm.course.form.FormatInfoWrapper"
                                              p:colSpan="3">
                                            <property name="layoutManager.lineGroupPrototype.header.headerText"
                                                      value="Format @{(#index + 1)}"/>
                                            <property name="layoutManager.lineGroupPrototype.header.style"
                                                      value="border-bottom : 1px solid black;"/>
                                            <property name="renderLineActions" value="false"/>
                                            <property name="renderAddLine" value="false"/>
                                            <property name="subCollections">
                                                <list>
                                                    <bean id="course_review_activity_details"
                                                          parent="Uif-StackedSubCollection-WithinSection"
                                                          p:collectionObjectClass="org.kuali.student.cm.course.form.ActivityInfoWrapper"
                                                          p:propertyName="activities" p:colSpan="3" p:align="left"
                                                          p:layoutManager.style="padding-left:0px;">
                                                        <property
                                                                name="layoutManager.lineGroupPrototype.header.headerText"
                                                                value="Activity @{(#index + 1)} (@{#line.activityType}) "/>
                                                        <property name="layoutManager.lineGroupPrototype.header.style"
                                                                  value="border-bottom : 1px dotted black;"/>
                                                        <property name="renderLineActions" value="false"/>
                                                        <property name="renderAddLine" value="false"/>
                                                        <property name="items">
                                                            <list>
                                                                <bean parent="CM-ReadOnlyInput" p:label="Contact Hours"
                                                                      p:fieldLabel.style="white-space : nowrap; margin-left : -4px;"
                                                                      p:propertyName="contactHours" p:colSpan="1"/>
                                                                <bean parent="CM-ReadOnlyInput" p:label="Duration"
                                                                      p:propertyName="durationCount" p:colSpan="1"/>
                                                                <bean parent="CM-ReadOnlyInput"
                                                                      p:label="Anticipated Class Size"
                                                                      p:fieldLabel.style="white-space : nowrap; margin-left : -4px;"
                                                                      p:propertyName="anticipatedClassSize"
                                                                      p:colSpan="1"/>
                                                            </list>
                                                        </property>
                                                    </bean>
                                                </list>
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>

                            <!-- This bean gets rendered if the outComes collection is empty. Otherwise, the one above does. -->
                            <bean parent="CM-ReadOnlyInput" p:label="Course Formats" id="emptyStringFormats"
                                  p:propertyName="reviewProposalDisplay.courseLogisticsSection.emptyStringFormats"
                                  p:render="@{#emptyList(#dataObject.reviewProposalDisplay.courseLogisticsSection.formatInfoWrappers)}">
                                <!-- Make required for state enroute  -->
                                <property name="simpleConstraint">
                                    <bean parent="RequiredConstraint">
                                        <property name="states">
                                            <list>
                                                <value>P</value>
                                            </list>
                                        </property>
                                    </bean>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="LearningObjectives-Review-section" parent="ReviewProposal-VerticalBoxSection">
