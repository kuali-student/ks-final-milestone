<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:dd="http://rice.kuali.org/dd"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                           http://rice.kuali.org/dd
                           http://rice.kuali.org/dd/dd.xsd">

	<bean id="SubjectCodeJoinOrgMaintenanceDocument" parent="SubjectCodeJoinOrgMaintenanceDocument-parentBean" />
	<bean id="SubjectCodeJoinOrgMaintenanceDocument-parentBean" abstract="true"
		parent="MaintenanceDocumentEntry" p:businessObjectClass="org.kuali.student.core.subjectcode.bo.SubjectCodeJoinOrg"
		p:maintainableClass="org.kuali.rice.kns.maintenance.KualiMaintainableImpl">
		<property name="documentTypeName" value="SubjectCodeJoinOrgMaintenanceDocument" />
		<property name="maintainableSections">
			<list>
				<ref bean="SubjectCodeJoinOrgMaintenanceDocument-subjectCodeJoinOrg" />
			</list>
		</property>
		<property name="lockingKeys">
			<list>
				<value>id</value>
			</list>
		</property>
	</bean>

	<!-- Maintenance Section Definitions -->
	<bean id="SubjectCodeJoinOrgMaintenanceDocument-subjectCodeJoinOrg" parent="SubjectCodeJoinOrgMaintenanceDocument-subjectCodeJoinOrg-parentBean" />
	<bean id="SubjectCodeJoinOrgMaintenanceDocument-subjectCodeJoinOrg-parentBean"
		abstract="true" parent="MaintainableSectionDefinition">
		<property name="title" value="Subject Code Department Maintenance" />
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="activeFromDate"
					p:required="true" />
				<bean parent="MaintainableFieldDefinition" p:name="activeToDate" />
				<bean parent="MaintainableFieldDefinition" p:name="active"
					p:required="true" />
				<bean parent="MaintainableFieldDefinition" p:name="orgId"
					p:required="true" />
			</list>
		</property>
	</bean>

</beans>