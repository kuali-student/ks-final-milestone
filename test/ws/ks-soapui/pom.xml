<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.kuali.student.test.ws</groupId>
    <artifactId>ws</artifactId>
    <version>1.0.0-SNAPSHOT</version>
  </parent>
  <groupId>org.kuali.student.test.ws.soapui</groupId>
  <artifactId>ks-soapui</artifactId>
  <version>1.0.0-SNAPSHOT</version>
  <packaging>jar</packaging>
  <name>WS - SoapUI</name>
  <description>This is a service testing framework based on SoapUI</description>
  <repositories>
    <!--
      Once the Kuali Master POM has been approved and published to the
      kuali.release repository, remove the kuali.release.lab repository entry
      from this pom
    -->
    <repository>
      <id>kuali.release.lab</id>
      <url>https://test.kuali.org/svn/student/deploymentlab/repository</url>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
    <repository>
      <id>kuali.release</id>
      <url>https://test.kuali.org/maven</url>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
    <repository>
      <id>eviwareRepository</id>
      <url>http://www.eviware.com/repository/maven2</url>
    </repository>
    <repository>
      <id>L2fprodMaven2Repository</id>
      <url>http://www.l2fprod.com/maven2</url>
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository>
      <id>eviwarePluginRepository</id>
      <url>http://www.eviware.com/repository/maven2</url>
    </pluginRepository>
  </pluginRepositories>
  <build>
    <plugins>
      <plugin>
        <groupId>org.kuali.student.test.ws</groupId>
        <artifactId>maven-soapui-plugin</artifactId>
        <version>3.5-kuali-1-SNAPSHOT</version>
        <configuration>
          <projectFile>${basedir}/src/main/resources/KualiStudent-soapui-project.xml</projectFile>
          <protocol>http</protocol>
          <host>localhost</host>
          <context>ks-stg</context>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>