<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.kuali.student</groupId>
    <artifactId>student-parent</artifactId>
    <version>1</version>
    <relativePath>../maven/student/pom.xml</relativePath>
  </parent>
  <groupId>org.kuali.student.test</groupId>
  <artifactId>test</artifactId>
  <packaging>pom</packaging>
  <name>Kuali Student Testing</name>
  <description>Kuali Student Testing</description>
  <version>1-SNAPSHOT</version>
  <repositories>
    <!--
      Once the Kuali Master POM has been approved and published to the
      kuali.release repository, remove the kuali.release.lab repository entry
      from this pom
    -->
    <repository>
      <id>kuali.release.lab</id>
      <url>https://test.kuali.org/svn/student/deploymentlab/repository</url>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
    <repository>
      <id>kuali.release</id>
      <url>https://test.kuali.org/maven</url>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
  </repositories>
  <modules>
    <!-- 
    <module>performance</module>
    <module>ui</module>
     -->
    <module>ws</module>
  </modules>
  <issueManagement>
    <system>JIRA</system>
    <url>https://test.kuali.org/jira/browse/KSLAB</url>
  </issueManagement>
  <scm>
    <connection>scm:svn:https://test.kuali.org/svn/student/deploymentlab/trunk/test</connection>
    <developerConnection>scm:svn:https://test.kuali.org/svn/student/deploymentlab/trunk/test</developerConnection>
    <url>https://test.kuali.org/svn/student/deploymentlab/trunk/test</url>
  </scm>
  <distributionManagement>
    <site>
      <id>ks.test.website</id>
      <url>file://C:/site/test</url>
    </site>
  </distributionManagement>
</project>