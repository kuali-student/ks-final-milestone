                                    </list>
                                </property>
                            </bean>

                            <!--Campus-->

                            <bean parent="Uif-HorizontalFieldGroup" p:fieldLabel.renderColon="false" p:style="padding-top: 10px;" p:group.validationMessages.render="false">
                                <property name="items">
                                    <list>
                                        <bean parent="Uif-HorizontalBoxGroup" p:validationMessages.render="false">
                                            <property name="cssClasses">
                                                <list merge="true">
                                                    <value>myplan-course-detail-label</value>
                                                </list>
                                            </property>
                                            <property name="items">
                                                <list>
                                                    <bean parent="Uif-Label" p:labelText="Campus"
