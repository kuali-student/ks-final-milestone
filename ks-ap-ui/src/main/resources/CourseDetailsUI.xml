                                                </list>
                                            </property>
                                        </bean>
                                        <bean parent="Uif-HorizontalBoxGroup" p:validationMessages.render="false">
                                            <property name="cssClasses">
                                                <list merge="true">
                                                    <value>myplan-course-detail-field</value>
                                                </list>
                                            </property>
                                            <property name="items">
