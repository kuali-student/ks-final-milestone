<project xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <artifactId>ks-with-rice-bundled</artifactId>
    <packaging>war</packaging>
    <name>KS with Rice Bundled</name>
    <description>Contains all KS modules and Rice</description>
    <parent>
        <groupId>org.kuali.student.web</groupId>
        <artifactId>ks-web</artifactId>
        <version>2.1.1-FR2-M1-build-641</version>
    </parent>
    <properties>
        <ks.gwt.module.org>org.kuali.student.core.organization.ui.OrgEntry</ks.gwt.module.org>
        <ks.gwt.module.lum>org.kuali.student.lum.lu.ui.main.LUMMain</ks.gwt.module.lum>
    </properties>
    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>gwt-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <goals>
                            <goal>compile</goal>
                        </goals>
                        <phase>${ks.gwt.compile.phase}</phase>
                    </execution>
                </executions>
                <configuration>
                    <modules>
                        <!--<module>${ks.gwt.module.org}</module> -->
                        <module>${ks.gwt.module.lum}</module>
                    </modules>
                </configuration>
            </plugin>
            <!-- added by bonnie -->
            <plugin>
                <groupId>org.mortbay.jetty</groupId>
                <artifactId>jetty-maven-plugin</artifactId>
                <configuration>
                    <systemProperties>
                        <systemProperty>
                            <name>jetty.port</name>
                            <value>8081</value>
                        </systemProperty>
                    </systemProperties>
                    <webAppConfig>
                        <contextPath>/ks-with-rice-bundled-dev</contextPath>
                        <!-- Remove after KULRICE-11157 is fixed -->
                        <maxFormContentSize>-1</maxFormContentSize>
                    </webAppConfig>
                    <stopKey>foo</stopKey>
                    <stopPort>9999</stopPort>
                </configuration>
                <dependencies>
                    <dependency>
                        <groupId>com.oracle</groupId>
                        <artifactId>${oracle.artifactId}</artifactId>
                        <version>${oracle.version}</version>
                    </dependency>
                    <dependency>
                        <groupId>mysql</groupId>
                        <artifactId>mysql-connector-java</artifactId>
                        <version>${mysql.version}</version>
                    </dependency>
                </dependencies>
            </plugin>
            <!-- end -->
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>tomcat-maven-plugin</artifactId>
                <version>1.0-beta-1</version>
                <configuration>
                    <path>/ks-with-rice-bundled-dev</path>
                    <port>8081</port>
                    <!--
            <path>/kr-dev</path>
            <port>8080</port>
          -->
                </configuration>
                <dependencies>
                    <dependency>
                        <groupId>com.oracle</groupId>
                        <artifactId>${oracle.artifactId}</artifactId>
                        <version>${oracle.version}</version>
                    </dependency>
                    <dependency>
                        <groupId>mysql</groupId>
                        <artifactId>mysql-connector-java</artifactId>
                        <version>${mysql.version}</version>
                    </dependency>
                </dependencies>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-war-plugin</artifactId>
                <configuration>
                    <webappDirectory>${project.build.directory}/${project.artifactId}-webapp</webappDirectory>
                    <dependentWarExcludes>**/web.xml,WEB-INF/lib/**,WEB-INF/krad-servlet.xml,**/sampleTravelApplication.tag,META-INF/**</dependentWarExcludes>
                    <overlays>
                        <!-- overlays are applied with a first-win strategy -->
                        <!-- specify order in which overlays are applied -->
                        <overlay>
                            <groupId>org.kuali.student.core</groupId>
                            <artifactId>ks-common-web</artifactId>
                        </overlay>
                        <overlay>
                            <!-- empty groupId/artifactId represents the current build -->
                        </overlay>
                        <overlay>
                            <groupId>org.kuali.rice</groupId>
                            <artifactId>rice-web</artifactId>
                            <excludes>
                                <exclude>plugins/tooltip/jquerybubblepopup-theme/kr-error-cs/**</exclude>
                                <!-- entries from dependentWarExcludes are repeated below -->
                                <exclude>**/web.xml</exclude>
                                <exclude>WEB-INF/lib/**</exclude>
                                <exclude>WEB-INF/krad-servlet.xml</exclude>
                                <exclude>**/sampleTravelApplication.tag</exclude>
                                <exclude>META-INF/**</exclude>
                            </excludes>
                        </overlay>
                        <overlay>
                            <groupId>org.kuali.rice</groupId>
                            <artifactId>rice-krad-web</artifactId>
                            <excludes>
                                <exclude>plugins/tooltip/jquerybubblepopup-theme/kr-error-cs/**</exclude>
                                <!-- entries from dependentWarExcludes are repeated below -->
                                <exclude>**/web.xml</exclude>
                                <exclude>WEB-INF/lib/**</exclude>
                                <exclude>WEB-INF/krad-servlet.xml</exclude>
                                <exclude>**/sampleTravelApplication.tag</exclude>
                                <exclude>META-INF/**</exclude>
                            </excludes>
                        </overlay>
                        <!-- any other remaining overlays will be applied last -->
                    </overlays>
                </configuration>
            </plugin>
        </plugins>
    </build>
