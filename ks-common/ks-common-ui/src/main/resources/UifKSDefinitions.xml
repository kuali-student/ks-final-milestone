    <bean id="KS-Uif-LookupView" abstract="true"
          class="org.kuali.student.common.uif.view.KSLookupView" parent="Uif-LookupView" p:stickyFooter="true">
        <property name="theme">
            <bean parent="KSTheme"/>
        </property>

        <!-- Need Krad Fix to work see KULRICE-9578, work around inplace-->
        <!--<property name="page.breadcrumbOptions.renderHomewardPathBreadcrumbs" value="@{!#form.renderedInLightBox}"/>-->
        <property name="onLoadScript"
                  value="@{#form.renderedInLightBox ? 'jQuery(&quot;#Uif-BreadcrumbWrapper&quot;).remove();' : ''}"/>
        <property name="rangeFieldGroupPrototype">
            <null/>
        </property>
        <!-- In KS, we need only the return value, not any other maintenance actions in lookup.-->
        <property name="resultsGroup.lineActions">
            <list>
                <bean parent="Uif-ActionLink">
                    <property name="actionLabel" value="Return Value"/>
                    <property name="finalizeMethodToCall" value="getReturnUrlForResults"/>
                    <property name="render"
                              value="@{!#empty(returnFormKey) and !#empty(returnLocation) and !view.hideReturnLinks}"/>
                </bean>
            </list>
        </property>
        <property name="resultsGroup.layoutManager.richTable.showSearchAndExportOptions" value="true"/>
        <property name="resultsGroup.layoutManager.richTable.templateOptions">
            <map merge="true">
                <entry key="sDom" value="iltp"/>
                <entry key="bPaginate" value="false"/>
            </map>
        </property>
        <property name="resultsGroup.collectionLookup">
            <null/>
        </property>
        <property name="criteriaGroup.footer.items">
          <list>
          </list>
        </property>
        <property name="footer">
            <bean parent="KS-Uif-FooterBase">
                <property name="items">
                    <list>
                        <bean parent="Uif-HorizontalBoxGroup">
                            <property name="items">
                                <list>
                                    <bean id="button_search" parent="Uif-PrimaryActionButton" p:methodToCall="search" p:actionLabel="Search"/>
                                    <bean parent="Uif-ActionLink" p:actionLabel="Cancel"
                                          p:render="@{#form.renderedInLightBox}"
                                          p:onClickScript="e.preventDefault();closeLightbox();return false;"
                                          p:additionalCssClasses="cancel_link"/>
                                </list>
                            </property>
                        </bean>
                    </list>
                </property>
            </bean>
        </property>
    </bean>

