    <bean id="KSTheme" abstract="true" parent="KSTheme-parentBean"/>
    <bean id="KSTheme-parentBean" parent="Uif-BaseTheme">
        <property name="cssFiles">
            <list merge="true">
                <value>plugins/tooltip/ks.jquery.bubblepopup.css</value>
                <value>themes/ksboot/stylesheets/enroll.css</value>
                <value>themes/ksboot/stylesheets/link-tabs.css</value>
            </list>
        </property>
        <!--TODO: KSENROLL-9721: Need to create a confirmation dialog in browser as opposed to make a server side round trip-->
        <!--We need to remove <value>themes/ksboot/scripts/krad.request.js</value> from the cript files after the rice fix-->
        <property name="scriptFiles">
            <list merge="true">
                <value>themes/bootstrap/scripts/bootstrap.js</value>
                <value>themes/ksboot/scripts/enroll.js</value>
                <value>krad/scripts/ks.krad.override.js</value>
                <value>themes/ksboot/scripts/krad.request.js</value>
                <value>krad/scripts/ks.krad.widget.js</value>
            </list>
        </property>
    </bean>

