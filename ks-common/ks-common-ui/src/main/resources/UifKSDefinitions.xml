    <bean id="KS-Uif-FormView" abstract="true" parent="Uif-FormView" class="org.kuali.student.common.uif.view.KSUifFormView">
        <property name="theme">
            <bean parent="KSTheme"/>
        </property>
        <property name="stickyTopGroup" value="false"/>
        <property name="stickyBreadcrumbs" value="false"/>
        <property name="stickyApplicationHeader" value="false"/>
        <property name="stickyHeader" value="true"/>
        <property name="applicationHeader">
            <bean parent="Uif-GlobalApplicationHeader-parentBean"
                  p:template="/ks-common/WEB-INF/ftl/ksApplicationHeader.ftl" p:render="true"
                  p:templateName="ks_applicationHeader"/>
        </property>
        <property name="header.cssClasses">
            <list merge="true">
                <value>ks-unified-header</value>
            </list>
        </property>
        <!-- Need Krad Fix to work see KULRICE-9578, work around inplace-->
        <!--<property name="page.breadcrumbOptions.renderHomewardPathBreadcrumbs" value="@{!#form.renderedInLightBox}"/>-->
        <property name="onLoadScript" value="@{#form.renderedInLightBox ? 'jQuery(&quot;#Uif-BreadcrumbWrapper&quot;).remove();' : ''}"/>
    </bean>


