<?xml version="1.0" encoding="UTF-8" ?>
<!-- Use this one for deploying to JBoss -->
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
	http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
             version="1.0">

    <!-- JPA test "unit" -->
    <persistence-unit name="Enum" transaction-type="JTA">
        <class>org.kuali.student.r2.common.entity.BaseAttributeEntity</class>

        <class>org.kuali.student.r2.core.class1.enumerationmanagement.model.EnumerationEntity</class>
        <class>org.kuali.student.r2.core.class1.enumerationmanagement.model.EnumerationAttributeEntity</class>
        <class>org.kuali.student.r2.core.class1.enumerationmanagement.model.EnumeratedValueEntity</class>
        <class>org.kuali.student.r2.core.class1.enumerationmanagement.model.EnumContextValueEntity</class>

        <exclude-unlisted-classes>true</exclude-unlisted-classes>

    </persistence-unit>


</persistence>
