<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

    <import resource="classpath:ks-data-dictionary-validator-configuration.xml"/>

    <bean id="mockLuiService" class="org.kuali.student.enrollment.classI.lui.mock.LuiServiceMockImpl" />
    <bean id="mockPermissionService" class="org.kuali.student.kim.permission.mock.PermissionServiceMockImpl" />
    <bean id="mockIdentityService" class="org.kuali.student.kim.identity.mock.IdentityServiceMockImpl" />

    <bean id="mockLprService" parent="mockLprValidationService" />
    <bean id="mockLprValidationService" class="org.kuali.student.enrollment.classI.lpr.mock.LuiPersonRelationServiceValidationDecorator" >
        <property name="nextDecorator" ref="mockLprPersistenceService"/>
        <property name="validator" ref="validator"/>
    </bean>
    <bean id="mockLprPersistenceService" class="org.kuali.student.enrollment.classI.lpr.mock.LuiPersonRelationServiceMockImpl"/>
        

    <bean id="lprDataDictionaryService" class="org.kuali.student.enrollment.classI.lpr.mock.LuiPersonRelationServiceDataDictionaryImpl">
        <property name="dataDictionaryService" ref="dataDictionaryServiceForLpr"/>
        <property name="nextDecorator" ref="mockLprPersistenceService"/>
    </bean>
     <bean id="dataDictionaryServiceForLpr" class="org.kuali.student.r2.common.datadictionary.DataDictionaryServiceImpl"/>
    
    <bean id="lprUnimplementedService" class="org.kuali.student.enrollment.classI.lpr.mock.LuiPersonRelationServiceUnimplemented"/>

</beans>