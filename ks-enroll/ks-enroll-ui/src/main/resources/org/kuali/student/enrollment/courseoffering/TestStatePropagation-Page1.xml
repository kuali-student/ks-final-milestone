                <property name="items">
                    <list>

                        <bean parent="Uif-HorizontalBoxSection" id="testButtonSection">
                            <property name="items">
                                <list>
                            <bean parent="Uif-PrimaryActionButton" p:performClientSideValidation="true" p:actionLabel="Test State Propagation"
