    <bean id="KS-CourseOfferingManagement-ExamOfferingPage" parent="KS-Uif-Page" p:disclosure.render="false"
          p:id="viewExamOfferingsPage" >
        <property name="editAuthz" value="true"/>
        <!-- <property name="onDocumentReadyScript" value="updateContextBar( 'ManageCourseOfferingManagement-ContextBar', 'KS-CourseOffering-View-ContextBar-PlaceHolder', @{#dp.contextBar.termDayOfYear}, '@{#ConfigProperties['application.url']}' );" /> -->
        <property name="onDocumentReadyScript" value="setSeasonalColor('ManageCourseOfferingManagement-ContextBar', '@{#dp.contextBar.termDayOfYear}', '@{#ConfigProperties['application.url']}'); updateViewHeaderText('@{currentCourseOfferingWrapper.ownerCode}'); enableDirtyCheck()" />
        <property name="pageSourceFile" value="CourseOfferingManagement-ManageCourseOfferingsPage.xml" /> <!-- Shows up in HTML so devs can find the code -->

        <property name="headerText" value="Exam Offerings"/>

        <property name="breadcrumbOptions.homewardPathBreadcrumbs">
            <list>
                <ref bean="KS-HomewardPathBreadcrumbs-Home"/>
                <ref bean="KS-HomewardPathBreadcrumbs-Enrollment"/>
                <bean parent="Uif-BreadcrumbItem">
                    <property name="label" value="Course Offerings"/>
                    <property name="url.viewId" value="courseOfferingManagementView"/>
                    <property name="url.controllerMapping" value="courseOfferingManagement"/>
                </bean>
                <bean parent="Uif-BreadcrumbItem">
                    <property name="label" value="@{#dp.subjectCode}"/>
                    <property name="url.viewId" value="courseOfferingManagementView"/>
                    <property name="url.controllerMapping" value="courseOfferingManagement"/>
                    <property name="url.requestParameters">
                        <map key-type="java.lang.String" merge="true">
                            <entry key="methodToCall" value="show"/>
                            <entry key="dataObjectClassName" value="org.kuali.student.r2.lum.course.dto.CourseInfo"/>
                            <entry key="hideReturnLink" value="true"/>
                            <entry key="inputCode" value="@{#dp.subjectCode}"/>
                            <entry key="termCode" value="@{#dp.termCode}"/>
                            <entry key="pageId" value="manageCourseOfferingsPage"/>
                            <entry key="viewName" value="@{#dp.subjectCode}"/>
                        </map>
                    </property>
                </bean>
                <bean parent="Uif-BreadcrumbItem">
                    <property name="label" value="@{#dp.inputCode}"/>
                    <property name="url.viewId" value="courseOfferingManagementView"/>
                    <property name="url.controllerMapping" value="courseOfferingManagement"/>
                    <property name="url.requestParameters">
                        <map key-type="java.lang.String" merge="true">
                            <entry key="methodToCall" value="show"/>
                            <entry key="dataObjectClassName" value="org.kuali.student.r2.lum.course.dto.CourseInfo"/>
                            <entry key="hideReturnLink" value="true"/>
                            <entry key="inputCode" value="@{#dp.inputCode}"/>
                            <entry key="termCode" value="@{#dp.termCode}"/>
                            <entry key="pageId" value="manageTheCourseOfferingPage"/>
                        </map>
                    </property>
                </bean>
            </list>
        </property>
        <property name="breadcrumbOptions.breadcrumbOverrides">
            <list>
                <bean parent="Uif-BreadcrumbItem">
                    <property name="label" value="Exam Offerings"/>
                </bean>
            </list>
        </property>

        <property name="items">
            <list>
                <bean parent="Uif-VerticalBoxSection">
                    <property name="items">
                        <list>
                            <!--<ref bean="KS-CourseOffering-ContextBar"/>-->
                            <ref bean="KS-CourseOfferingManagement-ExamOfferingByCOTableSection"/>
                            <ref bean="KS-CourseOfferingManagement-ExamOfferingByAOClustersSection" />
                            <bean parent="KS-CourseOfferingManagement-ExamOfferingTableSection-Cancelled" p:render="@{!#dp.examOfferingCancelledList.isEmpty()}">
                                <property name="propertyName" value="examOfferingCancelledList"/>
                                <property name="headerText" value="Cancelled Exam Offering for Course Offering"/>
                            </bean>
                            <bean parent="KS-CourseOfferingManagement-ExamOfferingByAOClustersSection"
                                  p:render="@{!#dp.eoCancelClusterList.isEmpty()}" >
                                <property name="headerText" value="Cancelled Exam Offerings for Activity Offerings"/>
                                <property name="propertyName" value="eoCancelClusterList" />
                                <property name="subCollections">
                                    <list>
                                        <bean parent="KS-CourseOfferingManagement-ExamOfferingByAOTableSection-Cancelled" p:render="true"/>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="footer">
            <bean parent="KS-Uif-FooterBase">
                <property name="items">
                    <list>
                        <bean parent="Uif-HorizontalBoxGroup" p:additionalCssClasses="ks-enrollment-footer-link">
                            <property name="items">
                                <list>
                                    <!--<bean parent="KS-Uif-PrimaryActionButton" p:id="saveExamOfferingRSI" p:methodToCall="saveExamOfferingRSI" p:actionLabel="Save" p:performClientSideValidation="true" p:render="@{!#view.readOnly}"/>-->
                                    <bean parent="KS-Uif-ActionLink" p:actionLabel="Return to Course Offering"
                                          p:methodToCall="show" />
                                </list>
                            </property>
                        </bean>
                    </list>
                </property>
            </bean>
        </property>
    </bean>

    <bean id="KS-CourseOfferingManagement-ExamOfferingTableSection-Cancelled" parent="Uif-TableCollectionSection"
          p:renderAddLine="false" p:renderLineActions="false"
          abstract="true">
        <property name="collectionObjectClass"
                  value="org.kuali.student.enrollment.class2.courseoffering.dto.ExamOfferingWrapper"/>
        <property name="collectionLookup">
            <bean parent="Uif-CollectionQuickFinder" p:quickfinderAction.render="false"/>
        </property>
        <property name="layoutManager.renderSequenceField" value="false"/>
        <property name="layoutManager.applyDefaultCellWidths" value="false"/>
        <property name="layoutManager.applyAlternatingRowStyles" value="false"/>
        <property name="layoutManager.richTable">
            <bean parent="KS-Uif-RichTable"/>
        </property>
        <property name="layoutManager.richTable.templateOptions">
            <map merge="true">
                <entry key="sDom" value="opir"/>
                <entry key="bPaginate" value="false"/>
                <entry key="bAutoWidth" value="false"/>
            </map>
        </property>
        <property name="items">
            <list>

                <bean parent="KS-Uif-DataField" p:label="Status" p:propertyName="labelState" p:escapeHtmlInPropertyValue="false"/>



                <bean parent="Uif-HorizontalFieldGroup" p:fieldLabel.labelText="Activity Code"
                      p:render="@{#lp.driverPerAO}">
                    <property name="items">
                        <list>
                            <bean parent="KS-Uif-DataField" p:propertyName="aoInfo.activityCode"
                                  p:fieldLabel.render="false">
                                <property name="inquiry">
                                    <bean parent="Uif-Inquiry"
                                          p:dataObjectClassName="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper"
                                          p:inquiryParameters="aoInfo.id"
                                          p:title="View Details"/>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <bean parent="KS-Uif-DataField-LabelTop" p:propertyName="typeName" p:label="Final Exam Driver Activity"
                      p:render="@{#lp.driverPerAO}"/>
                <bean id="cancelledEoOverrideMatrix" parent="KS-Uif-DataField" p:label="Override Matrix" p:propertyName="overrideMatrixUI"
                      p:render="@{(!#dp.examOfferingCancelledList.isEmpty() and #dp.currentCourseOfferingWrapper.matrixOveridableCODriven) or (!#dp.eoCancelClusterList.isEmpty() and #dp.currentCourseOfferingWrapper.matrixOveridableAODriven)}">
                    <property name="onDocumentReadyScript"
                              value="setInlineEditCheckboxReadonlyValue('@{#component.id}', @{#lp.overrideMatrix});"/>
                    <property name="additionalCssClasses">
                        <list merge="true">
                            <value>ks-fontello-icon-ok</value>
                            <value>clearfix</value>
                        </list>
                    </property>
                </bean>
                <bean parent="KS-Uif-DataField" p:label="Days" p:propertyName="requestedSchedule.daysUI"/>
                <bean parent="KS-Uif-DataField" p:label="Start Time" p:propertyName="requestedSchedule.startTimeUI" />
                <bean parent="KS-Uif-DataField" p:label="End Time" p:propertyName="requestedSchedule.endTimeUI"/>
                <bean parent="KS-Uif-DataField" p:label="Facility" p:propertyName="requestedSchedule.building.name"  />
                <bean parent="KS-Uif-DataField" p:label="Room" p:propertyName="requestedSchedule.roomCode" />
            </list>
        </property>
    </bean>

    <bean id="KS-CourseOfferingManagement-ExamOfferingTableSection" parent="Uif-TableCollectionSection" p:renderAddLine="false"
          abstract="true">
        <property name="collectionObjectClass"
                  value="org.kuali.student.enrollment.class2.courseoffering.dto.ExamOfferingWrapper"/>
        <property name="collectionLookup">
            <bean parent="Uif-CollectionQuickFinder" p:quickfinderAction.render="false"/>
        </property>
        <property name="onDocumentReadyScript" value="initInlineRowFields();"/>
        <property name="layoutManager.renderSequenceField" value="false"/>
        <property name="layoutManager.applyDefaultCellWidths" value="false"/>
        <property name="layoutManager.applyAlternatingRowStyles" value="false"/>
        <property name="layoutManager.richTable">
            <bean parent="KS-Uif-RichTable"/>
        </property>
        <property name="layoutManager.richTable.templateOptions">
            <map merge="true">
                <entry key="sDom" value="opir"/>
                <entry key="bPaginate" value="false"/>
                <entry key="aaSorting" value="[[1,'asc'],[2,'asc']]"/>
                <entry key="bAutoWidth" value="false"/>
            </map>
        </property>
        <property name="items">
            <list>
                <bean id="eoRsiStatus" parent="KS-Uif-DataField" p:label="Status" p:propertyName="labelState" p:escapeHtmlInPropertyValue="false">
                    <property name="onDocumentReadyScript" value="jQuery(this).find('label:contains(Status)').parent('th').addClass('uif-activity-offering-item-status');"/>
                </bean>

                <bean parent="Uif-HorizontalFieldGroup" p:fieldLabel.labelText="Activity Code" p:render="@{#lp.driverPerAO}">
                    <property name="onDocumentReadyScript" value="jQuery(this).find('label:contains(Activity Code)').parent('th').addClass('uif-activity-offering-item-ac');"/>
                    <property name="items">
                        <list>
                            <bean parent="KS-Uif-DataField" p:propertyName="aoInfo.activityCode" p:fieldLabel.render="false">
                                <property name="inquiry">
                                    <bean parent="Uif-Inquiry" p:dataObjectClassName=
                                        "org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper"
                                        p:inquiryParameters="aoInfo.id"
                                        p:title="View Details"/>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
                <bean parent="KS-Uif-DataField-LabelTop" p:propertyName="typeName" p:label="Final Exam Driver Activity" p:render="@{#lp.driverPerAO}"   >
                    <property name="onDocumentReadyScript" value="jQuery('#hiddenRefName').hide();jQuery(this).find('label:contains(Final Exam Driver Activity)').parent('th').addClass('uif-activity-offering-item-feac');"/>
                </bean>

                <bean parent="Uif-VerticalFieldGroup" p:label="Override Matrix"
                      p:render="@{(!#dp.examOfferingWrapperList.isEmpty() and #dp.currentCourseOfferingWrapper.matrixOveridableCODriven) or (!#dp.eoClusterResultList.isEmpty() and #dp.currentCourseOfferingWrapper.matrixOveridableAODriven)}">
                    <property name="onDocumentReadyScript" value="jQuery(this).find('label:contains(Override Matrix)').parent('th').addClass('uif-activity-offering-item-om');"/>
                    <property name="items">
                        <list>
                            <bean id="eoOverrideMatrix" parent="Uif-InputField" p:propertyName="overrideMatrix">
                                <property name="onChangeScript" value="matrixOverrideFlagChange(e, 'examOfferingRemoveMatrixOverrideConfirmDialog'); writeHiddenToForm(&quot;matrixOverrideCheckBoxId&quot;, &quot;@{#component.id}&quot;);"/>
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>off</value>
                                        <value>toggleable-element</value>
                                        <value>non-row-selector</value>
                                    </list>
                                </property>
                                <property name="readOnly" value="false"/>
                                <property name="control">
                                    <bean parent="Uif-CheckboxControl"/>
                                </property>
                            </bean>
                            <bean id="overrideMatrixUI_id" parent="KS-Uif-DataField" p:propertyName="overrideMatrixUI">
                                <property name="onDocumentReadyScript" value="setInlineEditCheckboxReadonlyValue('@{#component.id}', @{#lp.overrideMatrix});"/>
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>on</value>
                                        <value>toggleable-element</value>
                                        <value>ks-fontello-icon-ok</value>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-VerticalFieldGroup" p:label="Day">
                    <property name="onDocumentReadyScript" value="jQuery(this).find('label:contains(Day)').parent('th').addClass('uif-activity-offering-item-day');"/>
                    <property name="items">
                        <list>
                            <bean id="eoRsiDayInExamPeriod" parent="KS-Uif-InputField" p:propertyName="requestedSchedule.dayInExamPeriod" p:required="true">
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>off</value>
                                        <value>toggleable-element</value>
                                    </list>
                                </property>
                                <property name="readOnly" value="false"/>
                                <property name="control">
                                    <bean parent="KS-Uif-DropdownControl" p:disabled="@{!#root.editAuthz}"/>
                                </property>
                                <property name="optionsFinder">
                                    <bean class="org.kuali.student.enrollment.class2.courseoffering.keyvalue.DayInExamPeriodKeyValues" p:addBlankOption="true" />
                                </property>
                            </bean>
                            <bean id="daysUI_id" parent="KS-Uif-DataField" p:propertyName="requestedSchedule.daysUI">
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>on</value>
                                        <value>toggleable-element</value>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-HorizontalFieldGroup" p:label="Start Time">
                    <property name="onDocumentReadyScript" value="jQuery(this).find('label:contains(Start Time)').parent('th').addClass('uif-activity-offering-item-st');"/>
                    <property name="items">
                        <list>
                            <bean id="eoRsiStartTime" parent="KS-Uif-InputField"
                                  p:propertyName="requestedSchedule.startTime" p:cssClasses="new_ts"
                                  p:required="true" p:disableNativeAutocomplete="true">
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>off</value>
                                        <value>toggleable-element</value>
                                    </list>
                                </property>
                                <property name="control">
                                    <bean parent="KS-Uif-TextControl" p:size="8" p:maxLength="8" p:cssClasses="validTimeAMPMPattern" p:onBlurScript="timeFieldOnBlur(e);"/>
                                </property>
                                <!--<property name="constraintText" value="HH:MM"/>-->
                                <!--<property name="validCharactersConstraint">
                                    <bean parent="TimePatternConstraint" />
                                </property>-->
                            </bean>
                            <bean id="startTimeUI_id" parent="KS-Uif-DataField" p:propertyName="requestedSchedule.startTimeUI">
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>on</value>
                                        <value>toggleable-element</value>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-HorizontalFieldGroup" p:label="End Time">
                    <property name="onDocumentReadyScript" value="jQuery(this).find('label:contains(End Time)').parent('th').addClass('uif-activity-offering-item-et');"/>
                      <property name="items">
                        <list>
                            <bean id="eoRsiEndTime" parent="KS-Uif-InputField"
                                  p:propertyName="requestedSchedule.endTime" p:cssClasses="new_ts"
                                  p:required="true" p:disableNativeAutocomplete="true">
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>off</value>
                                        <value>toggleable-element</value>
                                    </list>
                                </property>
                                <property name="control">
                                    <bean parent="KS-Uif-TextControl" p:size="8" p:maxLength="8" p:cssClasses="validTimeAMPMPattern" p:onBlurScript="timeFieldOnBlur(e);"/>
                                </property>
                                <!--<property name="constraintText" value="HH:MM"/>-->
                                <!--<property name="validCharactersConstraint">
                                    <bean parent="TimePatternConstraint" />
                                </property>-->
                            </bean>
                            <bean id="endTimeUI_id" parent="KS-Uif-DataField" p:propertyName="requestedSchedule.endTimeUI">
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>on</value>
                                        <value>toggleable-element</value>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-VerticalFieldGroup" p:label="Facility">
                    <property name="onDocumentReadyScript" value="jQuery(this).find('label:contains(Facility)').parent('th').addClass('uif-activity-offering-item-facility');"/>
                    <property name="items">
                        <list>
                            <bean id="eoRsiBuilding" parent="KS-Uif-InputField-LabelTop"
                                  p:propertyName="requestedSchedule.buildingCode" p:uppercaseValue="true">
                                <!-- Rice 2.4 upgrade : removing p:labelPlacement="TOP" -->
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>off</value>
                                        <value>toggleable-element</value>
                                    </list>
                                </property>
                                <property name="control">
                                    <bean parent="KS-Uif-TextControl" p:size="20" p:cssClasses="new_rdl_components"/>
                                </property>
                                <!-- The facility code quick finder -->
                                <property name="quickfinder">
                                    <bean parent="Uif-QuickFinderByScript">
                                        <property name="dataObjectClassName" value="org.kuali.student.r2.core.room.dto.BuildingInfo"/>
                                        <property name="fieldConversions">
                                            <map>
                                                <entry key="buildingCode" value="requestedSchedule.buildingCode"/>
                                            </map>
                                        </property>
                                    </bean>
                                </property>
                                <!-- The facility code auto-complete -->
                                <property name="suggest">
                                    <bean parent="Uif-Suggest" p:render="true" p:valuePropertyName="buildingCode"
                                          p:suggestQuery.dataObjectClassName="org.kuali.student.r2.core.room.dto.BuildingInfo">
                                        <property name="suggestQuery">
                                            <bean parent="Uif-AttributeQueryConfig"
                                                  p:queryMethodToCall="retrieveBuildingInfoByCode"
                                                  p:queryFieldMapping="requestedSchedule.buildingCode:buildingCode"
                                                  p:returnFieldMapping="buildingCode:requestedSchedule.buildingCode,name:requestedSchedule.editRenderHelper.buildingName"/>
                                        </property>
                                        <property name="templateOptions">
                                            <map merge="true">
                                                <entry key="minLength" value="1"/>
                                            </map>
                                        </property>
                                    </bean>
                                </property>
                            </bean>
                            <bean id="buildingName_id" parent="KS-Uif-DataField" p:propertyName="requestedSchedule.building.name" p:fieldLabel.render="false" >
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>on</value>
                                        <value>toggleable-element</value>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-VerticalFieldGroup" p:label="Room">
                     <property name="onDocumentReadyScript" value="jQuery(this).find('label:contains(Room)').parent('th').addClass('uif-activity-offering-item-room');"/>
                     <property name="items">
                        <list>
                            <bean id="eoRsiRoom" parent="KS-Uif-InputField"
                                  p:propertyName="requestedSchedule.roomCode">
                                <property name="control">
                                    <bean parent="KS-Uif-TextControl" p:size="20" p:cssClasses="new_rdl_components"/>
                                </property>
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>off</value>
                                        <value>toggleable-element</value>
                                    </list>
                                </property>
                                <property name="quickfinder">
                                    <bean parent="Uif-QuickFinderByScript">
                                        <property name="dataObjectClassName" value="org.kuali.student.r2.core.room.dto.RoomInfo"/>
                                        <property name="autoSearch" value="true"/>
                                        <property name="fieldConversions">
                                            <map>
                                                <entry key="roomCode" value="requestedSchedule.roomCode"/>
                                            </map>
                                        </property>
                                        <property name="lookupParameters">
                                            <map>
                                                <entry key="requestedSchedule.buildingCode" value="buildingCode"/>
                                            </map>
                                        </property>
                                    </bean>
                                </property>
                                <property name="dependencyConstraints">
                                    <list>
                                        <bean parent="PrerequisiteConstraint"
                                              p:propertyName="requestedSchedule.buildingCode"/>
                                    </list>
                                </property>
                            </bean>
                            <bean id="roomCode_id" parent="KS-Uif-DataField" p:propertyName="requestedSchedule.roomCode">
                                <property name="additionalCssClasses">
                                    <list merge="true">
                                        <value>on</value>
                                        <value>toggleable-element</value>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="layoutManager.actionFieldPrototype">
            <bean parent="Uif-HorizontalFieldGroup" p:align="left" p:required="false" p:label="" p:style="min-width:70px;">
                <property name="onDocumentReadyScript" value="jQuery( 'th.uif-collection-column-action' ).addClass('uif-activity-offering-item-icon');jQuery('.uif-collection-column-action').find('a').addClass('uif-activity-offering-item-icon')"/>

            </bean>
        </property>
        <property name="lineActions">
            <list>
                <bean id="EO-toggleEditButton" parent="KS-Uif-ActionLink" p:actionLabel=""
                      p:additionalCssClasses="toggleable-element on" p:actionScript="editInlineRow(e);">
                    <property name="actionImage">
                        <bean parent="Uif-Image" p:altText="Edit" p:title="Edit"
                              p:source="" p:cssClasses="ks-fontello-icon-pencil"/>
                    </property>
                </bean>
                <bean id="EO-toggleUpdateButton" parent="KS-Uif-ActionLink" p:actionLabel=""
                      p:additionalCssClasses="toggleable-element off"
                      p:actionScript="saveInlineRSI(e, '@{#ConfigProperties['application.url']}');">
                    <property name="actionImage">
                        <bean parent="Uif-Image" p:altText="Update" p:title="Update"
                              p:source="" p:cssClasses="ks-fontello-icon-floppy"/>
                    </property>
                </bean>
                <bean id="EO-toggleCancelButton" parent="KS-Uif-ActionLink" p:actionLabel=""
                      p:additionalCssClasses="toggleable-element off" p:actionScript="cancelInlineRow(e);">
                    <property name="actionImage">
                        <bean parent="Uif-Image" p:altText="Cancel" p:title="Cancel"
                              p:source="" p:cssClasses="ks-fontello-icon-block-1"/>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KS-CourseOfferingManagement-ExamOfferingByCOTableSection" parent="KS-CourseOfferingManagement-ExamOfferingTableSection"
          p:render="@{!#dp.examOfferingWrapperList.isEmpty()}">
        <property name="headerText" value="Exam Offering for Course Offering"/>
        <property name="header.lowerGroup">
            <bean parent="Uif-VerticalBoxGroup">
                <property name="items">
                    <list>
                    	<!-- Rice 2.4 upgrade removing p:labelPlacement="LEFT" -->
                        <bean parent="KS-Uif-DataField"
                              p:label="Crosslisted as"
                              p:propertyName="currentCourseOfferingWrapper.alternateCOCodes"
                              p:render="@{currentCourseOfferingWrapper.alternateCOCodes.size() > 0 }"
                              p:readOnly="true"
                              p:additionalCssClasses="ks-uif-label-inline"/>
                    </list>
                </property>
            </bean>
        </property>
        <property name="propertyName" value="examOfferingWrapperList"/>
    </bean>

