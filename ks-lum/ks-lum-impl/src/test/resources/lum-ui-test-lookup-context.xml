		<property name="path" value="search.findCluSet" />
		<property name="initialLookup" ref="kuali.lu.lookup.findCluSet.suggest" />
		<property name="additionalLookups">
			<list>
				<ref bean="kuali.lu.lookup.findCluSet.advanced" />
			</list>
		</property>
	</bean>
