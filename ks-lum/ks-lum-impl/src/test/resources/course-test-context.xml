	</bean>

	<bean id="lrcServiceImpl"
		class="org.kuali.student.lum.lrc.service.impl.LrcServiceImpl">
		<property name="lrcDao" ref="lrcDao" />
        <property name="searchManager" ref="lrcSearchManager" />
	</bean>

	<!-- Search -->
