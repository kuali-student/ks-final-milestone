 <bean id="resultScaleInfo.typeKey-parent" abstract="true" parent="baseKualiTypeRequired">
  <property name="name" value="typeKey"/>
 </bean>
 <bean id="resultScaleInfo.typeKey" parent="resultScaleInfo.typeKey-parent"/>

