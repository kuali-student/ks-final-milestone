    <!--
        Each course details tab/section extends this
    -->
    <bean id="CM-Proposal-Course-View-VerticalBoxSection" parent="Uif-VerticalBoxSection" abstract="true">
        <property name="cssClasses">
            <list merge="true">
                <value>course_details_tab_section</value>
            </list>
        </property>
        <property name="header">
            <bean parent="Uif-HeaderTwo"/>
        </property>
        <!--  Turn off validation messages for the tab sections -->
        <property name="validationMessages" ref="KS-ValidationMessages-Off"/>
    </bean>

    <bean id="CM-Proposal-Course-View-HorizontalBoxSection" parent="Uif-HorizontalBoxSection" abstract="true"/>

    <!-- A component modifier which is can exclude specified items from the tab group. -->
    <bean id="CM-TabGroupTabsModifier" class="org.kuali.student.cm.course.modifiers.TabGroupTabsModifier" p:runPhase="FINALIZE" abstract="true">
        <property name="excludeItemIds">
            <list/>
        </property>
    </bean>

    <bean id="CM-Proposal-Course-Create-Page" parent="Uif-MaintenancePage" p:disclosure.render="false">
        <property name="header" ref="CM-Proposal-Course-Create-Header"/>
        <property name="headerText" value="@{(#isCreateProposal) ? 'Create Course' : 'Modify Course'}"/>
        <property name="footer.items">
            <list>
                <bean parent="CM-Proposal-Course-Create-Footer" p:render="@{(!#uiHelper.curriculumSpecialistUser) and (!#isModifyNoNewVersion)}"/>
                <bean parent="CM-Proposal-Course-Create-CurriculumSpecialistFooter" p:render="@{#uiHelper.curriculumSpecialistUser}"/>
            </list>
        </property>
        <property name="onDocumentReadyScript"
                  value="onCourseLoad(@{#uiHelper.curriculumSpecialistUser}, '@{#uiHelper.selectedSection.sectionId}')"/>
        <property name="items">
            <list>
                <bean parent="Uif-TabGroup" p:id="course_tabs" >
                <!--<bean parent="Uif-TabGroup" p:id="course_tabs"  p:render="@{!(#isModifyNoNewVersion and #isRetiredCourse)}"> -->
                    <property name="validationMessages.displayMessages" value="false"/>
                    <property name="validationMessages" ref="KS-ValidationMessages-Off"/>
                    <property name="items">
                        <list>
                            <ref bean="CM-Proposal-Course-CourseInfo-Section"/>
                            <ref bean="CM-Proposal-Course-Governance-Section"/>
                            <ref bean="CM-Proposal-Course-Logistics-Section"/>
                            <ref bean="CM-Proposal-Course-LearningObjective-Section"/>
                            <ref bean="CM-Proposal-CourseRequisites-Section"/>
                            <ref bean="CM-Proposal-Course-ActiveDates-Section"/>
                            <ref bean="CM-Proposal-Course-Financial-Section"/>
                            <ref bean="CM-Proposal-Course-AuthorsAndCollaborator-Section"/>
                            <ref bean="CM-Proposal-Course-SupportingDocument-Section"/>
                            <ref bean="CM-Course-Modify-RetireInfo-Section-parent"/>
                        </list>
                    </property>
                    <property name="tabsWidget.templateOptionsJSString">
                        <value>
                            { selected: @{#uiHelper.selectedSection.ordinal() - 1} }
                        </value>
                    </property>
                    <property name="tabsWidget.position" value="LEFT"/>

                    <property name="componentModifiers">
                        <list>
                            <!-- Exclude these sections for modify with no new version where course is retired -->
                            <bean parent="CM-TabGroupTabsModifier" p:runCondition="@{#isModifyNoNewVersion and ! #isRetiredCourse and !#isSupersededCourseFormerlyRetired}">
                                <property name="excludeItemIds">
                                    <list>
                                        <value>CM-Course-Modify-RetireInfo-Section-parent</value>
                                        <value>CM-Proposal-Course-AuthorsAndCollaborator-Section</value>
                                        <value>CM-Proposal-Course-SupportingDocument-Section</value>
                                    </list>
                                </property>
                            </bean>
                            <!-- Exclude these sections for modify with no new version where course is retired -->
                            <bean parent="CM-TabGroupTabsModifier" p:runCondition="@{#isModifyNoNewVersion and (#isRetiredCourse or #isSupersededCourseFormerlyRetired)}">
                                <property name="excludeItemIds">
                                    <list>
                                        <value>CM-Proposal-Course-AuthorsAndCollaborator-Section</value>
                                        <value>CM-Proposal-Course-SupportingDocument-Section</value>
                                    </list>
                                </property>
                            </bean>
                            <!-- Exclude these sections when the condition is NOT a modify with no new version (which
                                 would be adds and modifies with new version) -->
                            <bean parent="CM-TabGroupTabsModifier" p:runCondition="@{ ! #isModifyNoNewVersion}">
                                <property name="excludeItemIds">
                                    <list>
                                        <value>CM-Course-Modify-RetireInfo-Section-parent</value>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <!-- The text above the tab panel. This gets fixed positioning via JS. -->
                <bean id="CM-Proposal-Course-View-TabHeader" parent="Uif-VerticalBoxGroup"
                      p:render="@{!#uiHelper.adminProposal}" p:additionalCssClasses="proposal_tab_header_footer">
                    <property name="header">
                        <bean parent="Uif-HeaderFour">
                            <property name="cssClasses">
                                <list merge="true">
                                    <value>course_tabs_header_footer_text</value>
                                </list>
                            </property>
                            <property name="headerText" value="1. Define Course"/>
                        </bean>
                    </property>
                </bean>

                <!-- The text and link below the tab panel. This gets fixed positioning via JS. -->
                <bean id="CM-Proposal-Course-View-ReviewProposalLink" parent="Uif-VerticalBoxGroup"
                      p:render="@{(!#uiHelper.adminProposal) and !#workflowDoc.initiated}" p:additionalCssClasses="proposal_tab_header_footer">
                    <property name="header">
                        <bean id="CM-Proposal-Course-View-Tab-ReviewHeader" parent="Uif-HeaderFour">
                            <property name="cssClasses">
                                <list merge="true">
                                    <value>course_tabs_header_footer_text</value>
                                </list>
                            </property>
                            <property name="headerText" value="2. Review and Submit"/>
                        </bean>
                    </property>
                    <property name="items">
                        <list>
                            <bean parent="Uif-NavigationActionLink">
                                <property name="id" value="CM-Proposal-Course-ReviewProposalLink"/>
                                <property name="actionLabel" value="Review Proposal"/>
                                <property name="finalizeMethodToCall" value="buildProposalActionLink"/>
                                <property name="finalizeMethodAdditionalArguments">
                                    <list>
                                        <value>docHandler</value>
                                        <value>CM-Proposal-Review-Course-Page</value>
                                    </list>
                                </property>
                                <property name="actionParameters">
                                    <map>
                                        <entry key="pageId" value="CM-Proposal-Review-Course-Page"/>
                                    </map>
                                </property>
                                <property name="render" value="@{#actionFlags[#Constants.KUALI_ACTION_CAN_EDIT]}"/>
                            </bean>
                        </list>
                    </property>
                </bean>
                <ref bean="CM-ExportHiddenField"/>
            </list>
        </property>
    </bean>

    <bean id="CM-Proposal-Course-Create-SectionHeader" parent="Uif-HeaderTwo">
        <property name="cssClasses">
            <list merge="true">
                <value>ks-header</value>
            </list>
        </property>
    </bean>

    <bean id="CM-Proposal-Course-Create-Header" parent="Uif-HeaderTwo">
        <property name="lowerGroup">
            <ref bean="Uif-HeaderLowerGroup"/>
        </property>
        <property name="upperGroup">
            <bean id="CM-Proposal-Course-View-AdminMessageGroup" parent="Uif-VerticalBoxGroup">
                <property name="items">
                    <list>
                        <bean id="CM-Proposal-Course-View-AdminMessage-TreeExpandCollapse" parent="Uif-HorizontalFieldGroup"
                              p:showLabel="false" p:render="@{#uiHelper.curriculumSpecialistUser}"
                              p:additionalCssClasses="uif-validationMessages,uif-groupValidationMessages,uif-pageValidationMessages-warning,ks-subSection">
                            <property name="items">
                                <list>
                                    <bean parent="Uif-Message" p:id="CM-Proposal-Course-View-Admin-Message"
                                          p:messageText="Showing required and optional fields.">
                                    </bean>
                                    <bean parent="KS-Uif-ActionLink"
                                          p:id="CM-Proposal-Course-View-Admin-Message-Expand-Optional-Link"
                                          p:actionScript="showHideCreateCourseOptionalElements();"
                                          p:actionLabel="Show only required fields">
                                    </bean>
                                </list>
                            </property>
                        </bean>
                    </list>
                </property>
            </bean>
        </property>
    </bean>

    <bean id="CM-Proposal-Course-Create-CurriculumSpecialistFooter" parent="Uif-FooterBase">
        <property name="items">
            <list>
                <!-- only allow this button if use has blanket approver permission AND if the document type is 'modify this version' -->
                <bean parent="Uif-PrimaryActionButton"
                      p:additionalCssClasses="btn-success"
                      p:ajaxSubmit="false"
                      p:actionLabel="Save"
                      p:methodToCall="completeModifyThisVersion"
                      p:performClientSideValidation="true"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_BLANKET_APPROVE] and #isModifyNoNewVersion}"/>
                <bean parent="Uif-PrimaryActionButton"
                      p:additionalCssClasses="btn-success"
                      p:ajaxSubmit="false"
                      p:actionLabel="Approve and Activate"
                      p:methodToCall="approveAndActivate"
                      p:performClientSideValidation="true"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_BLANKET_APPROVE] and (!#isModifyNoNewVersion) and (!#workflowDoc.initiated) and #uiHelper.adminProposal}"/>
                <bean parent="Uif-SecondaryActionButton"
                      p:ajaxSubmit="false"
                      p:methodToCall="saveProposal"
                      p:performClientSideValidation="true"
                      p:actionLabel="Save Progress"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_SAVE] and (!#isModifyNoNewVersion)}"
                      p:jumpToIdAfterSubmit="TOP">
                </bean>
                <bean parent="Uif-ActionLink" p:id="CM-Proposal-Course-View-Admin-Cancel"
                      p:actionLabel="Cancel"
                      p:methodToCall="back"
                      p:style="margin-top:5px;"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CLOSE]}"/>
            </list>
        </property>
    </bean>

    <bean id="CM-Proposal-Course-Create-Footer" parent="CM-Proposal-Course-EditPage-Footer"/>

    <bean id="CM-Proposal-Course-EditPage-Footer" parent="CM-Proposal-Course-EditPage-Footer-parentBean"/>
    <bean id="CM-Proposal-Course-EditPage-Footer-parentBean" parent="Uif-FooterBase" abstract="true">
        <property name="items">
            <list>
                <bean parent="Uif-HorizontalBoxGroup">
                    <property name="items">
                        <list>
                            <bean parent="Uif-SaveAction"
                                  p:ajaxSubmit="false"
                                  p:methodToCall="saveProposal"
                                  p:actionLabel="Save and Continue"
                                  p:performClientSideValidation="true"
                                  p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_SAVE]}">
                                <property name="actionParameters">
                                    <map>
                                        <entry key="displayPage" value="NEXT"/>
                                    </map>
                                </property>
                            </bean>
                            <bean parent="Uif-SecondaryActionButton"
                                  p:ajaxSubmit="false"
                                  p:methodToCall="saveProposal"
                                  p:actionLabel="Save Progress"
                                  p:performClientSideValidation="true"
                                  p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_SAVE]}">
                            </bean>
                            <bean parent="Uif-ActionLink" p:id="CM-Proposal-Course-View-Cancel"
                                  p:actionLabel="Cancel"
                                  p:methodToCall="back"
                                  p:style="margin-top:5px;"
                                  p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CLOSE]}"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

</beans>
