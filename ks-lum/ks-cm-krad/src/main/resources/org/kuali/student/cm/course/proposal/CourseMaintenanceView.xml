<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--
        The data dictionary for the CourseInfoWrapper class.
    -->
    <bean id="CourseInfoWrapper" parent="CourseInfoWrapper-parentBean"/>
    <bean id="CourseInfoWrapper-parentBean" abstract="true" parent="DataObjectEntry">
        <property name="dataObjectClass" value="org.kuali.student.cm.course.form.wrapper.CourseInfoWrapper"/>
        <property name="titleAttribute" value="courseInfo.id"/>
        <property name="attributes">
            <list/>
        </property>
        <property name="complexAttributes">
            <list/>
        </property>
        <property name="objectLabel" value="CourseInfo Wrapper"/>
        <property name="primaryKeys">
            <list>
                <value>courseInfo.id</value>
            </list>
        </property>
    </bean>

    <!-- Controller Class: CourseController.java View Helper Class: CourseViewHelperServiceImpl.java UX : http://ux.ks.kuali.org.s3.amazonaws.com/wireframes/ENR/Scheduling_Interface/v3/start.html 
        This view has couple of pages (edit and copy) with all the beans defined at 1. CourseViewCourseInfoPage.xml -->

    <bean id="CM-Proposal-Course-Create-View" parent="KS-Uif-MaintenanceView">
        <property name="dataObjectClassName" value="org.kuali.student.cm.course.form.wrapper.CourseInfoWrapper"/>
        <!-- New Course header requires static string therefore below string construction is not required.
        commenting below code in case required in future.
         @{!#empty(document.newMaintainableObject.proposal.name) ? document.newMaintainableObject.proposal.name + ' (Proposal)': 'New Course (Proposal)'} -->
        <!--<property name="headerText" value="@{(document.newMaintainableObject.proposal != null and !#empty(document.newMaintainableObject.proposal.name)) ? document.newMaintainableObject.proposal.name + ' (Proposal)': 'New Course (Proposal)'}" />-->
        <property name="headerText"
                  value="@{(#uiHelper.headerText eq null or #uiHelper.headerText.length() lt 1) ? 'Create Course': #uiHelper.headerText }"/>
        <property name="singlePageView" value="false"/>
        <!-- Override the default to allow for navigation -->
        <property name="viewHelperServiceClass"
                  value="org.kuali.student.cm.course.service.impl.CourseMaintainableImpl"/>
        <property name="presentationControllerClass"
                  value="org.kuali.student.cm.uif.util.CurriculumManagementMaintenancePresentationControllerBase" />
        <property name="authorizerClass"
                  value="org.kuali.student.cm.course.auth.CurriculumManagementViewAuthorizerBase" />
        <property name="applyDirtyCheck" value="true"/>
        <property name="breadcrumbOptions.homewardPathBreadcrumbs">
            <list>
                <ref bean="KS-HomewardPathBreadcrumbs-Home"/>
                <ref bean="KS-HomewardPathBreadcrumbs-CurriculumManagement"/>
            </list>
        </property>
        <property name="breadcrumbOptions.breadcrumbOverrides">
            <list>
                <ref bean="KS-HomewardPathBreadcrumbs-ProposalName"/>
                <ref bean="KS-HomewardPathBreadcrumbs-CreateCourse"/>
            </list>
        </property>
        <property name="header.id" value="CM-Proposal-Course-Create-Header"/>
        <property name="stateMapping">
            <bean parent="WorkflowStateMapping">
                <property name="customClientSideValidationStates">
                    <map>
                        <entry key="I" value="S"/>
                        <entry key="S" value="S"/>
                        <entry key="R" value="S"/>
                        <entry key="P" value="S"/>
                    </map>
                </property>
            </bean>
        </property>

        <property name="dialogs">
            <list>
                <bean id="Proposal-Review-Course-ConfirmCancelProposal-Dialog" parent="CM-Proposal-Review-ConfirmCancelProposal-Dialog"/>
                <bean id="Proposal-Review-Course-ConfirmSubmit-Dialog" parent="CM-Proposal-Review-ConfirmSubmit-Dialog"/>
                <bean id="Proposal-Review-Course-ConfirmReturnToPreviousNode-Dialog" parent="CM-Proposal-Review-ConfirmReturnToPreviousNode-Dialog"/>
                <bean id="Proposal-Review-Course-ConfirmApprove-Dialog" parent="CM-Proposal-Review-ConfirmApprove-Dialog"/>
                <bean id="Proposal-Review-Course-ConfirmBlanketApprove-Dialog" parent="CM-Proposal-Review-ConfirmBlanketApprove-Dialog"/>
                <bean id="Proposal-Review-Course-ConfirmReject-Dialog" parent="CM-Proposal-Review-ConfirmReject-Dialog"/>
                <bean id="Proposal-Review-Course-ConfirmAcknowledge-Dialog" parent="CM-Proposal-Review-ConfirmAcknowledge-Dialog"/>
                <bean id="Proposal-Review-Course-ConfirmWithdraw-Dialog" parent="CM-Proposal-Review-ConfirmWithdraw-Dialog"/>
                <bean id="CM-Proposal-Course-Rule-CourseRangeLookup" parent="KRMS-CourseRangeResult-Lookup"/>
                <bean id="CM-Proposal-Course-Rule-WarningMessage" parent="KRMS-WarningMessagedYesNo"/>
            </list>
        </property>
        <property name="items">
            <list>
                <ref bean="CM-Proposal-Course-Create-Page"/>
                <ref bean="CM-Proposal-Review-Course-Page"/>
                <ref bean="KS-RuleMaintenance-Page-Parent"/>
            </list>
        </property>
        <property name="additionalScriptFiles">
            <list>
                <value>themes/ksboot/scripts/ruleMaintenance.js</value>
            </list>
        </property>
        <property name="additionalCssFiles">
            <list>
                <value>themes/ksboot/stylesheets/krms.css</value>
            </list>
        </property>
        <property name="expressionVariables">
            <map merge="true">
                <entry key="dataObject" value="document.newMaintainableObject.dataObject"/>
                <entry key="uiHelper" value="document.newMaintainableObject.dataObject.uiHelper"/>
                <entry key="workflowDoc" value="document.documentHeader.workflowDocument"/>
                <entry key="htmlUtils" value="T(org.springframework.web.util.HtmlUtils)"/>
                <entry key="isCompareView" value="pageId eq T(org.kuali.student.cm.common.util.CurriculumManagementConstants$CoursePageIds).REVIEW_COURSE_PROPOSAL_PAGE and document.oldMaintainableObject.dataObject.courseInfo.id != null"/>
                <entry key="isViewCourseView" value="viewId eq T(org.kuali.student.cm.common.util.CurriculumManagementConstants$CourseViewIds).VIEW_COURSE_VIEW"/>
                <entry key="isCreateProposal" value="document.newMaintainableObject.dataObject.uiHelper.isStringExistsInArray(docTypeName,T(org.kuali.student.cm.common.util.CurriculumManagementConstants$DocumentTypeNames).COURSE_CREATE_DOC_TYPE_NAMES)"/>
                <entry key="isModifyNoNewVersion" value="docTypeName eq T(org.kuali.student.cm.common.util.CurriculumManagementConstants$DocumentTypeNames$CourseProposal).COURSE_MODIFY_ADMIN_NOVERSION"/>
                <entry key="isRetiredCourse" value="document.newMaintainableObject.dataObject.courseInfo.stateKey eq T(org.kuali.student.r2.common.dto.DtoConstants).STATE_RETIRED"/>
                <entry key="isModifyReviewProposal" value="document.newMaintainableObject.dataObject.uiHelper.modifyWithNewVersionProposal and pageId eq T(org.kuali.student.cm.common.util.CurriculumManagementConstants$CoursePageIds).REVIEW_COURSE_PROPOSAL_PAGE"/>
                <entry key="isSupersededCourseFormerlyRetired" value="document.newMaintainableObject.dataObject.supersededCourseFormerlyRetired" />
            </map>
        </property>
        <property name="header.rightGroup">
            <ref bean="CM-Proposal-Course-Create-RightHeader"/>
        </property>
    </bean>

    <bean id="CM-Proposal-Course-Create-RightHeader" parent="KS-Unified-Header-Right-Group" p:additionalCssClasses="viewcourseview-header-status">
        <property name="items">
            <list>
                <ref bean="CM-Proposal-Course-Right-Header-Action-Group"/>
                <ref bean="CM-Proposal-Header-Right-Group-Status"/>
            </list>
        </property>
    </bean>

    <bean id="CM-Proposal-Course-Right-Header-Action-Group" parent="KS-Unified-Header-Action-Group"
          p:cssClasses="cm-unified-header-rightGroup-action">
        <property name="items">
            <list>
                <bean parent="CM-Proposal-Course-LinkGroup-CancelProposal" p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CANCEL] and (!#isModifyNoNewVersion)}"/>
                <bean parent="CM-Proposal-Course-LinkGroup-CopyCourse" p:methodToCall="copyProposal" p:render="@{#form.pageId eq 'CM-Proposal-Review-Course-Page' and !#uiHelper.modifyWithNewVersionProposal}"/>
                <bean parent="CM-Proposal-Course-LinkGroup-Comments" p:render="@{!#isModifyNoNewVersion}"/>
                <bean parent="CM-Proposal-Course-LinkGroup-Decisions" p:render="@{!#isModifyNoNewVersion}"/>
                <bean parent="CM-PrintAction" p:render="@{!#workflowDoc.initiated and !#isModifyReviewProposal}"/>
                <bean parent="CM-ExportAction" p:render="@{!#workflowDoc.initiated and !#isModifyReviewProposal}"/>
                <ref bean="CM-Export-ActionLinks"/> <!-- This is the content for the export action popover -->
            </list>
        </property>
    </bean>

    <bean id="CM-Proposal-Course-LinkGroup-CancelProposal" parent="CM-Proposal-Course-LinkGroup-CancelProposal-parent"/>

    <bean id="CM-Proposal-Course-LinkGroup-CancelProposal-parent" parent="KS-Uif-Cancel-NavigationActionLink" abstract="true"
          p:methodToCall="cancel"
          p:title="Cancel Proposal" p:disabled="@{#workflowDoc.initiated}"
          p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CANCEL]}">
    </bean>

    <bean id="CM-Proposal-Course-LinkGroup-Comments" parent="CM-Proposal-Course-LinkGroup-Comments-parent"/>

    <bean id="CM-Proposal-Course-LinkGroup-Comments-parent" parent="Uif-KS-NavigationActionLink-parentBean" abstract="true"
          p:actionScript="common.showViewInLightBox('@{#ConfigProperties['application.url']}/kr-krad/cm_comment?viewId=CM-Proposal-Course-Comment-View&amp;methodToCall=start&amp;proposalId=@{#dataObject.proposalInfo.id}');"
          p:disabled="@{#workflowDoc.initiated}" p:title="Comments">
        <property name="actionImage">
            <bean parent="Uif-Image" p:altText="Comments" p:cssClasses="ks-fontello-icon-comment" p:title="Comments"/>
        </property>
        <property name="actionImagePlacement" value="IMAGE_ONLY"/>
    </bean>

    <bean id="CM-Proposal-Course-LinkGroup-Decisions" parent="CM-Proposal-Course-LinkGroup-Decisions-parent"/>

    <bean id="CM-Proposal-Course-LinkGroup-Decisions-parent" parent="Uif-KS-NavigationActionLink-parentBean"  abstract="true"
          p:actionScript="common.showViewInLightBox('@{#ConfigProperties['application.url']}/kr-krad/cm_decision?viewId=CM-Proposal-Course-Decision-View&amp;methodToCall=start&amp;proposalId=@{#dataObject.proposalInfo.id}');"
          p:disabled="@{#workflowDoc.initiated}" p:title="Decisions">
        <property name="actionImage">
            <bean parent="Uif-Image" p:altText="Decisions" p:cssClasses="ks-fontello-icon-hammer" p:title="Decisions"/>
        </property>
        <property name="actionImagePlacement" value="IMAGE_ONLY"/>
    </bean>


    <bean id="CM-Proposal-Header-Right-Group-Status" parent="KS-Unified-Right-Header-Group-Status" p:render="@{!#workflowDoc.initiated and #dataObject.proposalInfo.meta!= null}">
        <property name="items">
            <list>
                <bean parent="Uif-HorizontalBoxGroup">
                    <property name="items">
                        <list>
                            <bean parent="Unified-Right-Header-Group-Status-Modified" p:messageText="@{(#dataObject.proposalInfo.meta eq null) ? '': 'Last modified by ' + {#dataObject.proposalInfo.meta.updateId} + ' on ' + {#dataObject.lastUpdated}}"/>
                            <bean parent="Uif-VerticalBoxGroup" p:id="CM-Proposal-Status-Lozenge">
                                <property name="items">
                                    <list>
                                        <bean id="CM-Proposal-Status-Draft-Lozenge" parent="KS-Lozenge-Grey-Uif-Message" p:messageText="Draft" p:render="@{#dataObject.proposalInfo.stateKey eq 'Saved'}"/>
                                        <bean id="CM-Proposal-Status-Cancelled-Lozenge" parent="KS-Lozenge-Red-Uif-Message" p:messageText="Cancelled" p:render="@{#dataObject.proposalInfo.stateKey eq 'Cancelled'}"/>
                                        <bean id="CM-Proposal-Status-Enroute-Lozenge" parent="KS-Lozenge-Teal-Uif-Message" p:messageText="Enroute" p:render="@{#dataObject.proposalInfo.stateKey eq 'Enroute'}"/>
                                        <bean id="CM-Proposal-Status-Withdrawn-Lozenge" parent="KS-Lozenge-Red-Uif-Message" p:messageText="Withdrawn" p:render="@{#dataObject.proposalInfo.stateKey eq 'Withdrawn'}"/>
                                        <bean id="CM-Proposal-Status-Approved-Lozenge" parent="KS-Lozenge-Teal-Uif-Message" p:messageText="Approved" p:render="@{#dataObject.proposalInfo.stateKey eq 'Approved'}"/>
                                        <bean id="CM-Proposal-Status-Rejected-Lozenge" parent="KS-Lozenge-Red-Uif-Message" p:messageText="Rejected" p:render="@{#dataObject.proposalInfo.stateKey eq 'Rejected'}"/>
                                        <bean id="CM-Proposal-Status-Exception-Lozenge" parent="KS-Lozenge-Red-Uif-Message" p:messageText="Exception" p:render="@{#dataObject.proposalInfo.stateKey eq 'Exception'}"/>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>


    <bean id="KS-HomewardPathBreadcrumbs-ProposalName" parent="Uif-BreadcrumbItem">
        <property name="label" value="@{#htmlUtils.htmlEscape(#uiHelper.headerText)}"/>
        <property name="url.viewId" value="startProposalView"/>
        <property name="url.controllerMapping" value="@{T(org.kuali.student.cm.common.util.CurriculumManagementConstants$ControllerRequestMappings).START_PROPOSAL}"/>
        <property name="url.requestParameters">
            <map key-type="java.lang.String" merge="true">
                <entry key="methodToCall" value="start"/>
                <entry key="hideReturnLink" value="true"/>
            </map>
        </property>
    </bean>

    <bean id="KS-HomewardPathBreadcrumbs-CreateCourse" parent="Uif-BreadcrumbItem">
        <property name="label" value="@{(#isCreateProposal) ? 'Create Course' : 'Modify Course'}"/>
    </bean>

    <bean id="CourseInfoMaintenanceDocument-Base" parent="CourseInfoMaintenanceDocument-Base-parent" abstract="true"/>

