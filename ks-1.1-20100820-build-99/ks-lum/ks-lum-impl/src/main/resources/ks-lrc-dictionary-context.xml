<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<import resource="classpath:ks-base-dictionary-context.xml" />

	<!-- ResultComponentInfo-->
	<bean id="org.kuali.student.lum.lrc.dto.ResultComponentInfo-parent"
		abstract="true" parent="objectStructureDefinition">
		<property name="name"
			value="org.kuali.student.lum.lrc.dto.ResultComponentInfo" />
		<property name="attributes">
			<list>
				<ref bean="resultComponentInfo.desc" />
				<ref bean="resultComponentInfo.effectiveDate" />
				<ref bean="resultComponentInfo.expirationDate" />
				<ref bean="resultComponentInfo.id" />
				<ref bean="resultComponentInfo.name" />
				<ref bean="resultComponentInfo.resultValues" />
				<ref bean="resultComponentInfo.state" />
				<ref bean="resultComponentInfo.type" />
			</list>
		</property>
		<property name="hasMetaData" value="true" />
	</bean>
	<bean id="org.kuali.student.lum.lrc.dto.ResultComponentInfo"
		parent="org.kuali.student.lum.lrc.dto.ResultComponentInfo-parent" />

	<bean id="resultComponentInfo.desc-parent" abstract="true" parent="baseComplex">
		<property name="name" value="desc" />
		<property name="dataObjectStructure" ref="org.kuali.student.core.dto.RichTextInfo" />
	</bean>
	<bean id="resultComponentInfo.desc" parent="resultComponentInfo.desc-parent" />

	<bean id="resultComponentInfo.effectiveDate-parent" abstract="true"
		parent="baseDate">
		<property name="name" value="effectiveDate" />
	</bean>
	<bean id="resultComponentInfo.effectiveDate" parent="resultComponentInfo.effectiveDate-parent" />

	<bean id="resultComponentInfo.expirationDate-parent" abstract="true"
		parent="baseDate">
		<property name="name" value="expirationDate" />
	</bean>
	<bean id="resultComponentInfo.expirationDate" parent="resultComponentInfo.expirationDate-parent" />

	<bean id="resultComponentInfo.id-parent" abstract="true" parent="baseString">
		<property name="name" value="id" />
	</bean>
	<bean id="resultComponentInfo.id" parent="resultComponentInfo.id-parent" />

	<bean id="resultComponentInfo.name-parent" abstract="true" parent="baseString">
		<property name="name" value="name" />
	</bean>
	<bean id="resultComponentInfo.name" parent="resultComponentInfo.name-parent" />

	<bean id="resultComponentInfo.resultValues-parent" abstract="true"
		parent="baseStringRepeating">
		<property name="name" value="resultValues" />
	</bean>
	<bean id="resultComponentInfo.resultValues" parent="resultComponentInfo.resultValues-parent" />

	<bean id="resultComponentInfo.state-parent" abstract="true"
		parent="baseString">
		<property name="name" value="state" />
	</bean>
	<bean id="resultComponentInfo.state" parent="resultComponentInfo.state-parent" />

	<bean id="resultComponentInfo.type-parent" abstract="true" parent="baseString">
		<property name="name" value="type" />
	</bean>
	<bean id="resultComponentInfo.type" parent="resultComponentInfo.type-parent" />

	<!-- RichTextInfo-->
	<bean id="org.kuali.student.core.dto.RichTextInfo-parent"
		abstract="true" parent="objectStructureDefinition">
		<property name="name" value="org.kuali.student.core.dto.RichTextInfo" />
		<property name="attributes">
			<list>
				<ref bean="richTextInfo.formatted" />
				<ref bean="richTextInfo.plain" />
			</list>
		</property>
		<property name="hasMetaData" value="true" />
	</bean>
	<bean id="org.kuali.student.core.dto.RichTextInfo" parent="org.kuali.student.core.dto.RichTextInfo-parent" />

	<bean id="richTextInfo.formatted-parent" abstract="true" parent="baseString">
		<property name="name" value="formatted" />
	</bean>
	<bean id="richTextInfo.formatted" parent="richTextInfo.formatted-parent" />

	<bean id="richTextInfo.plain-parent" abstract="true" parent="baseString">
		<property name="name" value="plain" />
	</bean>
	<bean id="richTextInfo.plain" parent="richTextInfo.plain-parent" />
</beans>